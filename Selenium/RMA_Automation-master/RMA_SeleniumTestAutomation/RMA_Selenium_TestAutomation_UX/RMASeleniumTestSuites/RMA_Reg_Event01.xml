<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="2" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Reg_Event01" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--Verify That Attachment,Event Status,Intervention Is Working In RMA Application -->
	<test name="RMA_Reg_Event_06">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Event_06" />
		</classes>
	</test>

	<!--Verify That Quality Management Record Is Working In RMA Application -->
	<test name="RMA_Reg_Event_07">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Event_07" />
		</classes>
	</test>

	<!--Verify That SMS Settings For Event Comments Are Working Properly In 
		RMA Application -->
	<test name="RMA_Reg_Event_08">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Event_08" />
		</classes>
	</test>

	<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
</suite>