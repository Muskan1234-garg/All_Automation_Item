<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_DailySmokeSuite_Funds" />

	<parameter name="DataSourceRow" value="1" /><!--To Set The Datasource 
		Row Name From The sheet RMA_TC_002 of File RMASeleniumAutomationTestData -->


	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<parameter name="ZoneName" value="Dashboard" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--Successful Corporate General Claim Creation On Providing Mandatory Data,Existence 
		of Necessary Tabs With Edit Of One Text Field In Each, Edit Of Created Claim 
		Is Validated -->
	<test name="RMA_Smoke_TC_002_GC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!-- Successful Reserve Creation, Reserve Grid Value Verification, Color 
		Code Change And Tool Tip Amount Of Reserve Progress Image On GC Claim Screen 
		Is Validated -->
	<test name="RMA_Smoke_TC_009">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_009" />
		</classes>
	</test>

	<!--Checks Not Attached to Claims CheckBox Function, Clear CheckBox On Transaction 
		Is Validated -->
	<test name="RMA_Reg_Funds_ClearCheck_TC_004">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ClearCheck_TC_004" />
		</classes>
	</test>

	<!--Checks Not Attached to Claims CheckBox Function Is Validated -->
	<test name="RMA_Reg_Funds_UnclearCheck_TC_004">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_UnclearCheck_TC_004" />
		</classes>
	</test>

	<!--Various Filters on Reset Printed Check Page(Check Date From, Check Date 
		To, Bank Account) Are Validated -->
	<test name="RMA_Reg_Funds_ResetCheck_TC_003">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ResetCheck_TC_003" />
		</classes>
	</test>

	<!--Successful Corporate General Claim Creation On Providing Mandatory Data,Existence 
		of Necessary Tabs With Edit Of One Text Field In Each, Edit Of Created Claim 
		Is Validated -->
	<test name="RMA_Smoke_TC_002_GC_1">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!--When Clear CheckBox Is UnChecked On Transaction, Then Check Is Not Visible 
		On Funds->UnClear Checks Page, No Cleared Date In Transaction History and 
		Funds History Is Validated -->
	<test name="RMA_Reg_Funds_UnclearCheck_TC_005">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_UnclearCheck_TC_005" />
		</classes>
	</test>

	<!--Visibility Of Printed Final/Supplemental Payment On UnClear Screen Is 
		Validated -->
	<test name="RMA_Reg_Funds_UnclearCheck_TC_007">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_UnclearCheck_TC_007" />
		</classes>
	</test>

	<!--Combined Payment Setup In RMA Application -->
	<test name="RMA_QTPRegression_TC_052_I">-->
		<classes>
			<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_052_I" />
		</classes>
	</test>

	<!--Successful Corporate General Claim Creation On Providing Mandatory Data,Existence 
		of Necessary Tabs With Edit Of One Text Field In Each, Edit Of Created Claim 
		Is Validated -->
	<test name="RMA_Smoke_TC_002_GC_3">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!--Reserve Creation In RMA Application For Making Auto Checks -->
	<test name="RMA_Smoke_TC_009_2">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_009" />
		</classes>
	</test>

	<!-- Combined Payment Creation In RMA Application -->
	<test name="RMA_Reg_CombinedPayment_TC_01">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_CombinedPayment_TC_01" />
		</classes>
	</test>

<!--Create Multiple F&F Payments With Different Claimants -->
	<test name="RMA_Reg_Funds_FirstAndFinalPayment_TC_013">
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_FirstAndFinalPayment_TC_013" />
		</classes>
	</test>
	
	<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
</suite>