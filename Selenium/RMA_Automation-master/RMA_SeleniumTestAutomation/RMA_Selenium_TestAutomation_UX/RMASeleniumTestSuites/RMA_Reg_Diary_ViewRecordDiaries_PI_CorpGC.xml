<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!-- Change The System Time To Eastern Standard Time Before Execution Of This Suite -->


	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Reg_Diary_ViewRecordDiaries_PI_CorpGC" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!-- Corporate General Claim Creation In RMA Application -->
	<test name="RMA_Smoke_TC_002_GC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!-- Attach Driver to Corporate General Claim Creation In RMA Application -->
	<test name="RMA_Regression_Diary_TC_06_Driver_GC">
		<parameter name="PersonInvolvedName" value="Driver" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_Diary_TC_06" />
		</classes>
	</test>

	<!-- Attach Employee to Corporate General Claim Creation In RMA Application -->
	<test name="RMA_Regression_Diary_TC_06_Employee_GC">
		<parameter name="PersonInvolvedName" value="Employee" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_Diary_TC_06" />
		</classes>
	</test>

	<!-- Attach Medical Staff to Corporate General Claim Creation In RMA Application -->
	<test name="RMA_Regression_Diary_TC_06_MedicalStaff_GC">
		<parameter name="PersonInvolvedName" value="MedicalStaff" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_Diary_TC_06" />
		</classes>
	</test>

	<!-- Attach Patient to Corporate General Claim Creation In RMA Application -->
	<test name="RMA_Regression_Diary_TC_06_Patient_GC">
		<parameter name="PersonInvolvedName" value="Patient" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_Diary_TC_06" />
		</classes>
	</test>

	<!-- Attach Physician to Corporate General Claim Creation In RMA Application -->
	<test name="RMA_Regression_Diary_TC_06_Physician_GC">
		<parameter name="PersonInvolvedName" value="Physician" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_Diary_TC_06" />
		</classes>
	</test>

	<!-- Attach Witness to Corporate General Claim Creation In RMA Application -->
	<test name="RMA_Regression_Diary_TC_06_Witness_GC">
		<parameter name="PersonInvolvedName" value="Witness" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_Diary_TC_06" />
		</classes>
	</test>

	<!-- Attach Other Person to Corporate General Claim Creation In RMA Application -->
	<test name="RMA_Regression_Diary_TC_06_OtherPerson_GC">
		<parameter name="PersonInvolvedName" value="OtherPerson" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_Diary_TC_06" />
		</classes>
	</test>
	<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
</suite>