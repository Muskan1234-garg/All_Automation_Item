	</test>	
		<!-- Successful Verify When Reserve Limits Are Exceeded CheckBox is OFF, Reserves Do Not Go On Hold Is Validated -->
	<test name="RMA_Reg_Super_ReserveLimit_TC_010">
	 	<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_010" />
		</classes>
	</test>	
	
		<!--Successful Verify When Reserve Limits Are Exceeded CheckBox is OFF, Reserves Do Not Go On Hold Is Validated -->
	<test name="RMA_Reg_Super_ReserveLimit_TC_011">
	 	<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_011" />
		</classes>
	</test>	
	<!--Successful Verify When Reserve Limits Are Exceeded CheckBox is OFF, Reserves Do Not Go On Hold Is Validated -->
	<test name="RMA_Reg_Super_ReserveLimit_TC_007">
	 	<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_007" />
