	<test name="RMA_Reg_Super_ReserveLimit_TC_006">
	 	<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_006" />
		</classes>
	</test>	
	<!--Successful Corporate General Claim Creation On Providing Mandatory Data, Existence of Necessary Tabs With Edit Of One Text Field In Each, Edit Of Created Claim Is Validated -->
	 <test name="RMA_Smoke_TC_002_GC_I">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>
		<test name="RMA_Reg_Super_ReserveLimit_TC_008">
	 	<classes>
