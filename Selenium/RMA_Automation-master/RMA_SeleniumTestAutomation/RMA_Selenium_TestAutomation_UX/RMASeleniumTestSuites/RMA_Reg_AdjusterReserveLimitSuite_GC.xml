<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">

<!--To Set The Executed SuiteName-->
<parameter name="ExecutedSuiteName" value="RMA_Reg_AdjusterReserveLimitSuite_GC" />
<parameter name="DataSourceRow" value="1" /><!--To Set The Datasource Row Name From The sheet RMA_TC_002 of File RMASeleniumAutomationTestData-->
 
<!--Verify That RMA Application Is Logged In-->
  <test name="RMA_TC_001">
    <classes>
      <class name="rmaseleniumtestscripts.RMA_TC_001"/>
    </classes>
  </test>
  
<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>	
	
<!--Successful New Users For Supervisory Chain Creation Is Validated Validated -->
<!--To Set The No. Of Users in supervisory chain-->
 <parameter name="NoOfUsers" value="3" />	 
	 <test name="RMA_Reg_Super_UserCreation_TC_001">	 
	   <parameter name="AdjusterLevel" value="Yes" />
			<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_UserCreation_TC_001" />
		</classes>
	</test> 
		
<!--Successful Set Reserve Limits At User And Validate It On Supervisory Chain -->
	 <!--To Set The LOB Based on claim-->

 	 	<parameter name="LOB" value="GC - General Claims" />
 	 	<parameter name="LimitFactor" value="10" />
	 <test name="RMA_Reg_Super_ReserveLimit_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_001" />
		</classes>
	</test>

<!--Successful Corporate General Claim Creation On Providing Mandatory Data, Existence of Necessary Tabs With Edit Of One Text Field In Each, Edit Of Created Claim Is Validated -->
	 <test name="RMA_Smoke_TC_002_GC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>
	<!--Successful Corporate General Claim Creation On Providing Mandatory Data, Existence of Necessary Tabs With Edit Of One Text Field In Each, Edit Of Created Claim Is Validated -->
	 <test name="RMA_Reg_Super_AdjusterOnClaim_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_AdjusterOnClaim_TC_001" />
		</classes>
	</test>

<!--Successful Reserve Can Be Created And Is Going On Hold Because Of Exceeded Reserve Limits Is Validated -->
	 <test name="RMA_Reg_Super_ReserveLimit_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_002" />
		</classes>
	</test>	
	<!--Successful Verify That The Mid Manager Can Have The Authority To Not Approve The Reserve Based On Reserve Limit Is Validated -->
	 <test name="RMA_Reg_Super_TransactionNotVisibleOnSAScreen_TC_001_UserMidManager">
	 <parameter name="Index" value="3" />	 
	<parameter name="ClassNameForUserName" value="rmaseleniumtestscripts.RMA_Reg_Super_UserCreation_TC_001::UserFirstName1[1]" />
	<parameter name="ClassNameForPassword" value="rmaseleniumtestscripts.RMA_Reg_Super_UserCreation_TC_001::SupervisoryUserPassword" />
	<parameter name="ClassNameForReserveType" value="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_002::ReserveTypeWhenManagerLimitsLess" />
	<parameter name="ClassNameForClaimNumber" value="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_001::SAClaimNumber" />
 	 	<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_TransactionNotVisibleOnSAScreen_TC_001" />
		</classes>
	</test>	
	<!--Successful Verify That The Top Manager Can Have The Authority To Not Approve The Reserve Based On Reserve Limit Is Validated -->
	 <test name="RMA_Reg_Super_TransactionNotVisibleOnSAScreen_TC_001_UserTopManager">
	 <parameter name="Index" value="3" />	 
	<parameter name="ClassNameForUserName" value="rmaseleniumtestscripts.RMA_Reg_Super_UserCreation_TC_001::UserFirstName1[0]" />
	<parameter name="ClassNameForPassword" value="rmaseleniumtestscripts.RMA_Reg_Super_UserCreation_TC_001::SupervisoryUserPassword" />
	<parameter name="ClassNameForReserveType" value="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_002::ReserveTypeWhenManagerLimitsLess" />
	<parameter name="ClassNameForClaimNumber" value="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_001::SAClaimNumber" />
 	 	<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_TransactionNotVisibleOnSAScreen_TC_001" />
		</classes>
	</test>	
		<!--Successful Verify That The Adjuster Manager Can Have The Authority To Not Approve The Reserve Based On Reserve Limit Is Validated -->
	 <test name="RMA_Reg_Super_TransactionNotVisibleOnSAScreen_TC_001_AdjManager">
	 <parameter name="Index" value="3" />	 
	<parameter name="ClassNameForUserName" value="rmaseleniumtestscripts.RMA_Reg_Super_UserCreation_TC_001::UserFirstName[0]" />
	<parameter name="ClassNameForPassword" value="rmaseleniumtestscripts.RMA_Reg_Super_UserCreation_TC_001::SupervisoryUserPassword" />
	<parameter name="ClassNameForReserveType" value="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_002::ReserveTypeWhenManagerLimitsLess" />
	<parameter name="ClassNameForClaimNumber" value="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_001::SAClaimNumber" />
 	 	<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_TransactionNotVisibleOnSAScreen_TC_001" />
		</classes>
	</test>	

<!--Successful Verify That The Manager Can Have The Authority To Either	Approve Or Not Approve The Reserve Based On Reserve Limit Is Validated -->
	 <test name="RMA_Reg_Super_ReserveLimit_TC_003">
   	<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_003" />
		</classes>
	</test>	
	
<!--Successful Verify Any User In Managerial Chain, Who Can See Reserve In Its Supervisory Approval View Can Reject the Reserve Irrespective Of Its	Limit Is Validated -->
