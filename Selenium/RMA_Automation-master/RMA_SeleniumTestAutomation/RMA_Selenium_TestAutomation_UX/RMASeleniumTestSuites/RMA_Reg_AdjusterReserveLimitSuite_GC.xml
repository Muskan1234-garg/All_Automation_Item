	 	<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_004" />
		</classes>
	</test>	

	
<!-- Successful Verify Change Of Reserve Status As 'Open' When Manager Approve/Reject The Reserve And Also Manager Can Reject The Reserve Based On Reserve Limit Is Validated -->
	<test name="RMA_Reg_Super_ReserveLimit_TC_005">
	 	<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_005" />
		</classes>
	</test>	
	<!--Successful Verify When Reserve Limits Are Exceeded CheckBox is OFF, Reserves Do Not Go On Hold Is Validated -->
	<!-- <test name="RMA_Reg_Super_ReserveLimit_TC_009">
	 	<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_009" />
		</classes>
	</test>	 -->
