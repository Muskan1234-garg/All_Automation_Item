<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

 <!-- Change The System Time To Eastern Standard Time Before Execution Of This Suite -->

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Reg_GST_Suite_GC_02" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>
	
	<!--Verify Entity Is Created Successfully With GST Details -->
	<test name="RMA_Reg_GST_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_GST_TC_001" />
		</classes>
	</test>	
	
	<!--Successful Corporate General Claim Creation Is Validated -->
	<test name="RMA_Smoke_TC_002_GC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>	
	
	<!--Verify Details On GST Details Tab For A Collection Is Validated-->
	<test name="RMA_Reg_GST_TC_012">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_GST_TC_012" />
		</classes>
	</test>
	
	<!--Verify GST Details With Settlement Payment CheckBox Is OFF, On Auto Check With ITC Entitlement (Payor/Insurer) Is 100% And On Its Third Party Payment Screen With ITC Entitlement (Payor/Insurer) is 40% Is Validated-->
	<test name="RMA_Reg_GST_TC_013">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_GST_TC_013" />
		</classes>
	</test>
	
	<!--Verify GST Details With Settlement Payment CheckBox Is ON, On Auto Check With ITC Entitlement (Payee) Is 70% Without Excess And On Its Third Party Payment Screen With ITC Entitlement (Payee) is 100% Without Excess Is Validated-->
	<test name="RMA_Reg_GST_TC_014">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_GST_TC_014" />
		</classes>
	</test>
	
	<!--Verify GST Details With Settlement Payment CheckBox Is ON, On Auto Check With ITC Entitlement (Payee) Is 100% With An Excess On Insurance Amount And On Its Third Party Payment Screen With ITC Entitlement (Payee) is 60% Without Excess Is Validated-->
	<test name="RMA_Reg_GST_TC_015">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_GST_TC_015" />
		</classes>
	</test>
	
	<!--Verify GST Details With Settlement Payment CheckBox Is ON, On Auto Check With ITC Entitlement (Payee) Is 0% Without Excess And On Its Third Party Payment Screen With ITC Entitlement (Payee) is 50% With An Excess On Insurance Amount Is Validated-->
	<test name="RMA_Reg_GST_TC_016">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_GST_TC_016" />
		</classes>
	</test>
	
	<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>

