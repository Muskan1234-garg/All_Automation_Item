<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!-- Change The System Time To Eastern Standard Time Before Execution Of 
		This Suite -->

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Suite_01_PrintCheck_TaskManager_Corporate_GC" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--Corporate General Claim Creation In RMA Application -->
	<test name="RMA_Smoke_TC_002_GC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!--Printing Of An Auto-check From Task-Manager Is Validated -->
	<test name="RMA_Reg_Funds_PrintCheck_TM_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_PrintCheck_TM_TC_001" />
		</classes>
	</test>

	<!--Printing Of Roll-Up Checks From Task-Manager Is Validated -->
	<test name="RMA_Reg_Funds_PrintCheck_TM_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_PrintCheck_TM_TC_002" />
		</classes>
	</test>

	<!--Printing Of Checks With Sub-Bank Accounts From Task-Manager Is Validated -->
	<test name="RMA_Reg_Funds_PrintCheck_TM_TC_003">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_PrintCheck_TM_TC_003" />
		</classes>
	</test>

	<!--Printing Of Checks With With Account Having Multiple Check Stocks From 
		Task-Manager Is Validated -->
	<test name="RMA_Reg_Funds_PrintCheck_TM_TC_004">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_PrintCheck_TM_TC_004" />
		</classes>
	</test>

	<!--Printing Checks When Printing Option Is Set "TO PRINTER ONLY" From Task 
		Manager Is Validated -->
	<test name="RMA_Reg_Funds_PrintCheck_TM_TC_005">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_PrintCheck_TM_TC_005" />
		</classes>
	</test>

	<!--Printing Checks When Printing Option Is Set "TO PRINTER AND FILE ONLY" 
		From Task Manager Is Validated -->
	<test name="RMA_Reg_Funds_PrintCheck_TM_TC_006">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_PrintCheck_TM_TC_006" />
		</classes>
	</test>

	<!--Printing Checks When Printing Option Is Set "TO FILE ONLY" From Task 
		Manager Is Validated -->
	<test name="RMA_Reg_Funds_PrintCheck_TM_TC_007">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_PrintCheck_TM_TC_007" />
		</classes>
	</test>

	<!--Printing Checks When Printing Option Is Set "TO CUSTOM FILE ONLY" From 
		Task Manager Is Validated -->
	<test name="RMA_Reg_Funds_PrintCheck_TM_TC_008">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_PrintCheck_TM_TC_008" />
		</classes>
	</test>

	<!--Printing Checks When Printing Option Is Set "TO PRINTER AND CUSTOM FILE" 
		From Task Manager Is Validated -->
	<test name="RMA_Reg_Funds_PrintCheck_TM_TC_009">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_PrintCheck_TM_TC_009" />
		</classes>
	</test>

	<!--Printing EFT Checks From Task Manager Is Validated -->
	<test name="RMA_Reg_Funds_PrintCheck_TM_TC_010">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_PrintCheck_TM_TC_010" />
		</classes>
	</test>

	<!--Printing Of Checks With Account Having Multiple Check Stocks From Funds->Print 
		Checks Is Validated -->
	<test name="RMA_Reg_PrintChecks_TC_11">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_PrintChecks_TC_11" />
		</classes>
	</test>

<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>

