<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!-- Change The System Time To Eastern Standard Time Before Execution Of 
		This Suite -->

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Reg_Suite_ReserveLimit_ReserveWorksheet_01" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>
	
	<!--Reserve WorkSheet Settings For GC From Line Of Business Setup And Reserve 
		WorkSheet Configuration Setting From Utilities Is Validated -->
	<test name="RMA_Reg_Funds_ReserveWorkSheet_Settings">
		<parameter name="LOB" value="GC - General Claims" />
		<parameter name="numofreserves" value="2" />
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorkSheet_Settings" />
		</classes>
	</test>

	<!--Successful New Users For Supervisory Chain Creation Is Validated Validated -->
	<!--To Set The No. Of Users in supervisory chain -->
	<parameter name="NoOfUsers" value="3" />
	<test name="RMA_Reg_Super_UserCreation_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_UserCreation_TC_001" />
		</classes>
	</test>

	<!-- Verify Settings For Utilities and Security Are Reverted -->
	<test name="RMA_Reg_Super_DeSelectALLSetting_TC_001">
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Super_DeSelectALLSetting_TC_001" />
		</classes>
	</test>

	<!--Successful Set Reserve Limits At User And Validate It On Supervisory 
		Chain -->
	<!--To Set The LOB Based on claim -->
	<parameter name="LOB" value="GC - General Claims" />
	<test name="RMA_Reg_Super_ReserveLimit_TC_001">
	<parameter name="LimitFactor" value="10"/>
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_001" />
		</classes>
	</test>

	<!--Successful Corporate General Claim Creation On Providing Mandatory Data, 
		Existence of Necessary Tabs With Edit Of One Text Field In Each, Edit Of 
		Created Claim Is Validated -->
	<test name="RMA_Smoke_TC_002_GC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!--Verification That Reserve WorkSheet Is Send To User's Manager When Exceeds 
		User Authority Limit And Manager Is Able To Approve It -->
	<test name="RMA_Reg_Funds_ReserveWorksheet_TC_010">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorksheet_TC_010" />
		</classes>
	</test>

	<!--Verification That Reserve WorkSheet Is Send To User's Manager When Exceeds 
		User Authority Limit And Manager Is Able To Reject It -->
	<test name="RMA_Reg_Funds_ReserveWorksheet_TC_011">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorksheet_TC_011" />
		</classes>
	</test>

	<!--Verification That Reserve WorkSheet Creation Is Giving Error When Settings 
		Are Unchecked From Utilities -->
	<test name="RMA_Reg_Funds_ReserveWorksheet_TC_012">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorksheet_TC_012" />
		</classes>
	</test>

	<!--Successful Set Reserve Limits At User And Validate It On Supervisory 
		Chain -->
	<!--To Set The LOB Based on claim -->
	<parameter name="LOB" value="GC - General Claims" />
	<test name="RMA_Reg_Super_ReserveLimit_TC_001_1">
	<parameter name="LimitFactor" value="10"/>
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_001" />
		</classes>
	</test>

	<!--Successful Corporate General Claim Creation On Providing Mandatory Data, 
		Existence of Necessary Tabs With Edit Of One Text Field In Each, Edit Of 
		Created Claim Is Validated -->
	<test name="RMA_Smoke_TC_002_GC_1">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!--Verification That Reserve WorkSheet Creation Is Not Going On Hold When 
		Settings Are Unchecked From Security->User Privileges Even When Settings 
		From Utilities Are Checked -->
	<test name="RMA_Reg_Funds_ReserveWorksheet_TC_013">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorksheet_TC_013" />
		</classes>
	</test>

	<!--Verify Behavior Of 'Restrict Unspecified Users' Setting For Reserve 
		WorkSheet -->
	<test name="RMA_Reg_Funds_ReserveWorksheet_TC_014">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorksheet_TC_014" />
		</classes>
	</test>

	<!--Verify top level and LOB level additional settings for Supervisory Approval 
		Application when Notify Immediate supervisor setting is ON -->
	<test name="RMA_Reg_Funds_ReserveWorksheet_TC_015">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorksheet_TC_015" />
		</classes>
	</test>

	<!--Verify top level and LOB level additional settings for Supervisory Approval 
		Application when Notify Immediate supervisor setting is OFF -->
	<test name="RMA_Reg_Funds_ReserveWorksheet_TC_016">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorksheet_TC_016" />
		</classes>
	</test>

	<!-- Verify Settings For Utilities and Security Are Reverted -->
	<test name="RMA_Reg_Super_DeSelectALLSetting_TC_001_1">
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Super_DeSelectALLSetting_TC_001" />
		</classes>
	</test>
	
	<!-- Revert Reserve WorkSheet Settings From Line Of Business Setup -->
	<test name="RMA_Reg_Funds_ReserveWorkSheet_RevertSettings">
		<parameter name="LOB" value="GC - General Claims" />
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorkSheet_RevertSettings" />
		</classes>
	</test>
<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>