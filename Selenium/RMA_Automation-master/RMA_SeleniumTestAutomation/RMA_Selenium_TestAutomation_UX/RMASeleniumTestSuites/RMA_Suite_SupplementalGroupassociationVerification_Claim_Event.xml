			<class name="rmaseleniumtestscripts.RMA_Reg_SupplementalGroupAssociation_TC_001" />
		</classes>
	</test>
	
	<!--General Claim Getting Created By Entering All Mandatory Fields Is Validated -->
	<test name="RMA_Smoke_TC_002_GC">
