			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>	
	
	<!-- Verify The Claims Supplemental Fields With Field Type Number,Entity,Multi-Entity,Event Number Lookup,Hyperlink On The Basis Of Value Selected In Group Associated Field -->
	<test name="RMA_Reg_SupplementalGroupAssociation_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_SupplementalGroupAssociation_TC_002" />
		</classes>
	</test>	
	
	<!--General Claim Getting Created By Entering All Mandatory Fields Is Validated -->
	<test name="RMA_Smoke_TC_002_WC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_WC" />
		</classes>
	</test>	
	
	<!-- Verify The Claims Supplemental Fields With Field Type Number,Entity,Multi-Entity,Event Number Lookup,Hyperlink On The Basis Of Value Selected In Group Associated Field -->
	<test name="RMA_Reg_SupplementalGroupAssociation_TC_002_01">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_SupplementalGroupAssociation_TC_002" />
		</classes>
	</test>	
	
