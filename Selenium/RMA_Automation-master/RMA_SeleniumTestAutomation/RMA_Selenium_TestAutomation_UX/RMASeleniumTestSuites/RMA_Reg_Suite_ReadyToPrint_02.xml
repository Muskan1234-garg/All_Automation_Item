<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!-- Change The System Time To Eastern Standard Time Before Execution Of 
		This Suite -->

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Reg_Suite_ReadyToPrint_02" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--Successful Corporate General Claim Creation On Providing Mandatory Data,Existence 
		of Necessary Tabs With Edit Of One Text Field In Each, Edit Of Created Claim 
		Is Validated -->
	<test name="RMA_Smoke_TC_002_GC_1">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!-- Navigation To Maintenance Screen -->
	<test name="RMA_Smoke_TC_041_I">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_041_I" />
		</classes>
	</test>

	<!-- Verify Recipient (Tax Collecting Entity) Created For Withholding Setup -->
	<test name="RMA_Reg_Funds_Withholding_RecipientCreation">
		<parameter name="NoOfWithholdingRecipient" value="1" />
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_Withholding_RecipientCreation" />
		</classes>
	</test>

	<!-- Verifying Ready To Print Functionality For Withholding Payments -->
	<test name="RMA_Reg_Funds_ReadyToPrint_TC_007">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReadyToPrint_TC_007" />
		</classes>
	</test>

	<!--Verify Entity Is Created Successfully With GST Details -->
	<test name="RMA_Reg_GST_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_GST_TC_001" />
		</classes>
	</test>

	<!--Successful Corporate General Claim Creation Is Validated -->
	<test name="RMA_Smoke_TC_002_GC_01">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!--Verifying Ready To Print Functionality For GST Payments -->
	<test name="RMA_Reg_Funds_ReadyToPrint_TC_008">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReadyToPrint_TC_008" />
		</classes>
	</test>

	<!--Verify That User Can Logged out From Application -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
</suite>