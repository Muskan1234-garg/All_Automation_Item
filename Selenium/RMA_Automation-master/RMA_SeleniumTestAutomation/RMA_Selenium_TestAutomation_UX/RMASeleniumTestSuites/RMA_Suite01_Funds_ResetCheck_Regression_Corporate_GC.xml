<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName"
		value="RMA_Suite01_Funds_ResetCheck_Regression_Corporate_GC" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--SMS Settings On Reset Printed Checks Are Validated -->
	<test name="RMA_Reg_Funds_ResetCheck_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ResetCheck_TC_001" />
		</classes>
	</test>

	<!--General Claim Getting Created By Entering All Mandatory Fields Is Validated -->
	<test name="RMA_Smoke_TC_002_GC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!--Various Filters on Reset Printed Check Page(Check Date From, Check Date 
		To, Bank Account) Are Validated -->
	<test name="RMA_Reg_Funds_ResetCheck_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ResetCheck_TC_002" />
		</classes>
	</test>

	<!--When Printed Check with Other Than Default Distribution Type is Reset 
		Through Reset Printed Check Then Its Distribution Type Changes To Default 
		And Check Status To Released Is Validated -->
	<test name="RMA_Reg_Funds_ResetCheck_TC_003">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ResetCheck_TC_003" />
		</classes>
	</test>
<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>