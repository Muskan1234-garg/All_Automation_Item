<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Suite_SupplementalGroupassociationVerification_MaintenanceScreens" />


	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
<!-- 	<parameter name="ZoneName" value="Maintenance" /> -->
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>			
	
	<!--Successful New Users For Powerview Creation Is Validated -->
	<!--To Set The No. Of Users As Per The Requirement -->
	<parameter name="NoOfUsers" value="1" />
	<test name="RMA_Reg_Super_UserCreation_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_UserCreation_TC_001" />
		</classes>
	</test>	
			
	<!--Verify That A PowerView Is Created And Created User Is Given Permission 
		To That PowerView -->
	<test name="RMA_Reg_PowerviewCreation_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_PowerviewCreation_TC_001" />
		</classes>
	</test>
	
	<!--Successful Logout And Login With Selected Zone Is Validated -->
	 <test name="RMA_Reg_SuperGen_LogoutLoginWithZone_TC_001_01">
	 <parameter name="ClassNameForUserName" value="rmaseleniumtestscripts.RMA_Reg_PowerviewCreation_TC_001::UserName" />
	 <parameter name="ClassNameForPassword" value="rmaseleniumtestscripts.RMA_Reg_PowerviewCreation_TC_001::Password" />
	  <parameter name="ClassNameForZoneName" value="Maintenance" />
	 	<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_SuperGen_LogoutLoginWithZone_TC_001" />
		</classes>
	</test>	

	<!-- Verify The Employee Supplemental Fields With Field Type Number,Entity,Multi-Entity,Event Number Lookup,Hyperlink On The Basis Of Value Selected In Group Associated Field -->
	<test name="RMA_Reg_SupplementalGroupAssociation_TC_007">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_SupplementalGroupAssociation_TC_007" />
		</classes>
	</test>
	
	<!-- Verify The Entity Supplemental Fields With Field Type Free Text On The Basis Of Value Selected In Group Associated Field -->
	<test name="RMA_Reg_SupplementalGroupAssociation_TC_008">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_SupplementalGroupAssociation_TC_008" />
		</classes>
	</test>
	
	<!-- Verify The Org Hierarchy Supplemental Fields With Field Type Multi Text/Codes On The Basis Of Value Selected In Group Associated Field -->
	<test name="RMA_Reg_SupplementalGroupAssociation_TC_009">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_SupplementalGroupAssociation_TC_009" />
		</classes>
	</test>
	
	<!-- Verify The Vehicle Supplemental Fields With Field Type Multi-Code, On The Basis Of Value Selected In Group Associated Field -->
	<test name="RMA_Reg_SupplementalGroupAssociation_TC_006">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_SupplementalGroupAssociation_TC_006" />
		</classes>
	</test>
<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>
