<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Suite01_Maint_Vehicle_Regression_02" />

	<!-- Test Case RMA_QTPRegression_TC_029_C Is Required To Reset The OLD UI Settings After The Execution Of RMA_QTPRegression_TC_027 & RMA_QTPRegression_TC_029 -->

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>
	
	 <!--Verify When User Changes The Order Of ToolbarButton In UX PV Then The Same Is Reflected In UX-->
	<test name="RMA_reg_Maint_Vehicle_TC011">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Vehicle_TC_011" />
		</classes>
	</test>
	
		
	<!--Verify That Page Cannot Be Displayed In Power View Must Be Displayed When Inspections Page Is Not Added In Power View  -->
	<test name="RMA_Reg_Maint_Vehicle_TC_012">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Vehicle_TC_012" />
		</classes>
	</test> 
	<!--Verify User Is Not Able To Remove Required Field From PowerView Editor Vehicle Screen-->
	<test name="RMA_Reg_Maint_Vehicle_TC_013">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Vehicle_TC_013" />
		</classes>
	</test> 
	<!--Verify User Is Able To Make Any Field Required,ReadOnly From PowerView Editor Vehicle-->
	<test name="RMA_Reg_Maint_Vehicle_TC_014">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Vehicle_TC_014" />
		</classes>
	</test> 
	<!--Verify That Help Is Displayed On Vehicle Screen If Added Via PowerView For Any Field-->
	<test name="RMA_Reg_Maint_Vehicle_TC_015">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Vehicle_TC_015" />
		</classes>
	</test> 
	
	
	<!--Verify That Validation Is Displayed When User Has Not Entered Data In Any Required Field On Vehicle -->
	<test name="RMA_Reg_Maint_Vehicle_TC_016">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Vehicle_TC_016" />
		</classes>
	</test>

	<!-- Verify Child Menu Inspections Is Working As Per Changes In PowerView On Vehicle -->
	<test name="RMA_Reg_Maint_Vehicle_TC_018">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Vehicle_TC_018" />
		</classes>
	</test>
	<!--Verify Child Menu Accident Is Working As Per Changes In PowerView On Vehicle -->
	<test name="RMA_Reg_Maint_Vehicle_TC_026">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Vehicle_TC_026" />
		</classes>
	</test>
	
	<!--Verify That ToolBar Buttons On Vehicle Screen In PowerView Are Working Fine -->
	<test name="RMA_Reg_Maint_Vehicle_TC_017">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Vehicle_TC_017" />
		</classes>
	</test>
	
	<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>