<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="2" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_DailySmoke_ClaimPC" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>


	<!--Successful Workers Compensation Claim Creation On Providing Mandatory 
		Data, Existence of Necessary Tabs With Data Entry Of One Text Field In Each, 
		Edit Of Created Claim Is Validated -->
	<test name="RMA_Smoke_TC_002_PC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_PC" />
		</classes>
	</test>

	<!--Successful Claimant Creation On Providing Mandatory Data, Existence 
		of Necessary Tabs With Edit Of One Text Field In Each, Edit Of Created Claimant 
		Is Validated -->
	<test name="RMA_Smoke_TC_004">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_004" />
		</classes>
	</test>

	<!--Successful Subrogation Creation On Providing Mandatory Data, Existence 
		of Necessary Tabs With Edit Of One Text Field In Each, Edit And Delete Of 
		Created Subrogation Is Validated -->
	<test name="RMA_Smoke_TC_007">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_007" />
		</classes>
	</test>

	<!--Create, Edit And Delete Of MedicalStaff As Person Involved For Created 
		WC Claim Is Validated -->
	<test name="RMA_SeleniumRegression_TC_027_WorkersCompensationClaim">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_SeleniumRegression_TC_027" />
		</classes>
	</test>

	<!-- Successful Reserve Creation On WC, Reserve Grid Value Verification, 
		Color Code Change And Tool Tip Amount Of Reserve Progress Image On Claim 
		Screen Is Validated -->
	<test name="RMA_Smoke_TC_009_PC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_009" />
		</classes>
	</test>

	<!--Successful Payment Addition On Worker Compensation Claim Is Done -->
	<test name="RMA_Smoke_TC_011_WC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_011" />
		</classes>
	</test>

	<!--Successful Payment Addition On Worker Compensation Claim Is Validated -->
	<test name="RMA_Smoke_TC_011GridVerification_WC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_011GridVerification" />
		</classes>
	</test>

	<!--Successful Collection Addition On Worker Compensation Claim Is Done -->
	<test name="RMA_Smoke_TC_014_WC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_014" />
		</classes>
	</test>

	<!--Successful Collection Addition On Worker Compensation Claim Is Validated -->
	<test name="RMA_Smoke_TC_014GridVerification_WC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_014GridVerification" />
		</classes>
	</test>

<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>

</suite>