<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Suite06_Maint_State_Regression_02" />


	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!-- Verification Of Availability of New State In Table After Creation on 
		State Maintenance, Claim and Event Page -->
	<test name="RMA_Reg_Maint_State_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_State_TC_001" />
		</classes>
	</test>

	<!-- Verification Of Changes Made On Editing State Is Available In Table 
		on State Maintenance,Claim and Event Page -->
	<test name="RMA_Reg_Maint_State_TC_005">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_State_TC_005" />
		</classes>
	</test>

	<!-- Verification that Deleted State Is Not Visible On Claims & Event Screens 
		And Deleted State Can Again Be Added On State Maintenance Page -->
	<test name="RMA_Reg_Maint_State_TC_007">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_State_TC_007" />
		</classes>
	</test>

	<!-- Verification That FROI Field Takes Only Non Negative Value While Creating 
		New States on State Maintenance page -->
	<test name="RMA_Reg_Maint_State_TC_003">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_State_TC_003" />
		</classes>
	</test>

<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>
