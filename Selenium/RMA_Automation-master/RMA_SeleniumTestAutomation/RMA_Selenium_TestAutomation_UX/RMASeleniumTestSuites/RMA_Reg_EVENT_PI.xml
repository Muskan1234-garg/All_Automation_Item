<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
<parameter name="DataSourceRow" value="2" />

<!--To Set The Executed SuiteName-->
<parameter name="ExecutedSuiteName" value="RMA_Reg_Event" />

<!--Verify That RMA Application Is Logged In-->
  <test name="RMA_TC_001">
    <classes>
      <class name="rmaseleniumtestscripts.RMA_TC_001"/>
    </classes>
  </test>
  
<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

<!--Verify That Event Is Created In RMA Application -->
	
<test name="RMA_Smoke_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_001" />
		</classes>
	</test>
<!--Verify That PI Employee Is Created , Edited and Deleted In RMA Application -->
	<test name="RMA_Reg_Event_PI_TC_001">
		<classes>
		<parameter name="Event_PI_Type" value="Employee" />
			<class name="rmaseleniumtestscripts.RMA_Reg_Event_PI_TC_001" />
		</classes>
	</test>
<!--Verify That PI Driver Is Created , Edited and Deleted In RMA Application -->
	<test name="RMA_Reg_Event_PI_TC_002">
		<classes>
		<parameter name="Event_PI_Type" value="Driver" />
			<class name="rmaseleniumtestscripts.RMA_Reg_Event_PI_TC_001" />
		</classes>
	</test>
<!--Verify That PI Witness Is Created , Edited and Deleted In RMA Application -->
	
	<test name="RMA_Reg_Event_PI_TC_003">
		<classes>
		<parameter name="Event_PI_Type" value="Witness" />
			<class name="rmaseleniumtestscripts.RMA_Reg_Event_PI_TC_001" />
		</classes>
	</test>
<!--Verify That PI OtherPerson Is Created , Edited and Deleted In RMA Application -->
	
	<test name="RMA_Reg_Event_PI_TC_004">
		<classes>
		<parameter name="Event_PI_Type" value="OtherPerson" />
			<class name="rmaseleniumtestscripts.RMA_Reg_Event_PI_TC_001" />
		</classes>
	</test>
<!--Verify That PI Patient Is Created , Edited and Deleted In RMA Application -->

	<test name="RMA_Reg_Event_PI_TC_005">
		<classes>
		<parameter name="Event_PI_Type" value="Patient" />
			<class name="rmaseleniumtestscripts.RMA_Reg_Event_PI_TC_001" />
		</classes>
	</test>
<!--Verify That PI Physician Is Created , Edited and Deleted In RMA Application -->

	<test name="RMA_Reg_Event_PI_TC_006">
		<classes>
		<parameter name="Event_PI_Type" value="Physician" />
			<class name="rmaseleniumtestscripts.RMA_Reg_Event_PI_TC_001" />
		</classes>
	</test>
<!--Verify That PI MedicalStaff Is Created , Edited and Deleted In RMA Application -->

	<test name="RMA_Reg_Event_PI_TC_007">
		<classes>
		<parameter name="Event_PI_Type" value="MedicalStaff" />
			<class name="rmaseleniumtestscripts.RMA_Reg_Event_PI_TC_001" />
		</classes>
	</test>
<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>

</suite>