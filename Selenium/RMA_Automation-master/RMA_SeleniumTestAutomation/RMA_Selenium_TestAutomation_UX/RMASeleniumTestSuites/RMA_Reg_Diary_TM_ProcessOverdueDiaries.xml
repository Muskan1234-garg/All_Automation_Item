<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Reg_Diary_TM_ProcessOverdueDiaries" />

	<parameter name="DataSourceRow" value="1" /><!--To Set The Datasource 
		Row Name From The sheet RMA_TC_002 of File RMASeleniumAutomationTestData -->


	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>
	<!--Successful New Users For Supervisory Chain Creation Is Validated Validated -->
	<!--To Set The No. Of Users in supervisory chain -->
	<parameter name="NoOfUsers" value="2" />
	<test name="RMA_Reg_Super_UserCreation_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_UserCreation_TC_001" />
		</classes>
	</test>

	<!-- Verify The Functionality Of Auto Late Notification When The Assigned 
		User Is Not The User That Issued The Diary Through Task Manager. -->
	<test name="RMA_Regression_Diary_TC_08">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_Diary_TC_08" />
		</classes>
	</test>

	<!-- Verify The Functionality Of Auto Late Notification When The Assigned 
		User Is Same As The User That Issued The Diary Through Task Manager. -->
	<test name="RMA_Regression_Diary_TC_09">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_Diary_TC_09" />
		</classes>
	</test>

<!-- 	Verify The Functionality Of Process Overdue Diaries And Verify System -->
<!-- 	Diaries Are Generated If System Diary CheckBox IS Checked. -->
	<test name="RMA_Regression_Diary_TC_13">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_Diary_TC_13" />
		</classes>
	</test>

<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
</suite>

