<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="5" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Suite_21_QTPReg_08_RollUp_02"/>

	<!-- Test Case RMA_QTPRegression_TC_052_I Is Required To Set Up Combined Payee For Roll Up Before The Execution Of Suite07-->

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>
	
	<!-- Setting Up Combined Payee For Roll Up Is Validated -->
	<test name="RMA_QTPRegression_TC_052_I">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_052_I" />
		</classes>
	</test>

	<!-- Payments Are Not Rolled Up For Print Check To File Options : 'TO PRINTER ONLY' For The Same Payee When 'Roll Up Checks To The Same Payee' Is Not Checked Is Validated -->
	<test name="RMA_QTPRegression_TC_055">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_055" />
		</classes>
	</test>

	<!-- Verification Of Payments For The Same Payee Are Not Rolled Up For Print Check To File Options : 'TO FILE ONLY' When 'Roll Up Checks To The Same Payee' Is Not Checked -->
	<test name="RMA_QTPRegression_TC_056">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_056" />
		</classes>
	</test>
		
<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>

