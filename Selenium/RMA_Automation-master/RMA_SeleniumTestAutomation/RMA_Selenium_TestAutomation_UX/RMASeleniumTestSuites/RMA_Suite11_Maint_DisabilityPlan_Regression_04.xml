<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Suite11_Maint_DisabilityPlan_Regression_04" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--Verify Maintenance Screen Navigation In RMA Application -->
	<test name="RMA_Smoke_TC_041_I">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_041_I" />
		</classes>
	</test>

	<!--Verify User Is Able To Add Data In All The Fields On Disability Plan Management Page And Create A Monthly Disability Plan -->
	<test name="RMA_Reg_Maint_DisabilityPlan_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_DisabilityPlan_TC_002" />
		</classes>
	</test>
	
	<!--Various Fields, Tabs And Creation Of Plan Class(Monthly) With Mandatory Fields On Disability Plan Management Are Validated -->
	<test name="RMA_Reg_Maint_DisabilityPlan_TC_012">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_DisabilityPlan_TC_012" />
		</classes>
	</test>

	<!--Verify Radio Buttons On Benefit Calculation Tab On Disability Plan Class Screen -->
	<test name="RMA_Reg_Maint_DisabilityPlan_TC_016">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_DisabilityPlan_TC_016" />
		</classes>
	</test>
	
	<!--Verify Table Driven Benefits Button Is Present And User Is Able To Add Data In Table Driven Benefits Window On Disability Plan Class -->
	<test name="RMA_Reg_Maint_DisabilityPlan_TC_017">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_DisabilityPlan_TC_017" />
		</classes>
	</test>
	
	<!--Verify Alternate Waiting Periods Button Is Present And User Is Able To Add Data In Alternate Waiting Periods Window On Disability Plan Class -->
	<test name="RMA_Reg_Maint_DisabilityPlan_TC_018">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_DisabilityPlan_TC_018" />
		</classes>
	</test>
	
	<!--Verify User Is Able To Delete A Disability Plan -->
	<test name="RMA_Reg_Maint_DisabilityPlan_TC_015">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_DisabilityPlan_TC_015" />
		</classes>
	</test>
<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>