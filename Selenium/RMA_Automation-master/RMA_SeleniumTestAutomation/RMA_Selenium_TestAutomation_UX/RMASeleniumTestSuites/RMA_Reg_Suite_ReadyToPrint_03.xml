<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="2" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Reg_Suite_ReadyToPrint_03" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--Successful Corporate General Claim Creation On Providing Mandatory Data, 
		Existence of Necessary Tabs With Edit Of One Text Field In Each, Edit Of 
		Created Claim Is Validated -->
	<test name="RMA_Smoke_TC_002_GC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>
	
	<!-- Successful Reserve Creation, Reserve Grid Value Verification, Color 
		Code Change And Tool Tip Amount Of Reserve Progress Image On Claim Screen 
		Is Validated -->
	<test name="RMA_Smoke_TC_009_GC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_009" />
		</classes>
	</test>
	
	<!--Verify That User Is Able To Mark A Payment As Ready To Print From Funds PreCheck--> 
	<test name="RMA_Reg_Funds_ReadyToPrint_TC_010">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReadyToPrint_TC_010" />
		</classes>
	</test>

	<!-- Navigation To Maintenance Screen -->
	<test name="RMA_Smoke_TC_041_I">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_041_I" />
		</classes>
	</test>

	<!-- Verify Recipient (Tax Collecting Entity) Created For Withholding Setup -->
	<test name="RMA_Reg_Funds_Withholding_RecipientCreation">
		<parameter name="NoOfWithholdingRecipient" value="1" />
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_Withholding_RecipientCreation" />
		</classes>
	</test>

	<!-- Verifying Ready To Print Functionality For Withholding Payments From Funds PreCheck-->
	<test name="RMA_Reg_Funds_ReadyToPrint_TC_011">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReadyToPrint_TC_011" />
		</classes>
	</test>


	<!--Verify Entity Is Created Successfully With GST Details -->
	<test name="RMA_Reg_GST_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_GST_TC_001" />
		</classes>
	</test>

	<!--Successful Corporate General Claim Creation Is Validated -->
	<test name="RMA_Smoke_TC_002_GC_01">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!--Verifying Ready To Print Functionality For GST Payments From Funds PreCheck-->
	<test name="RMA_Reg_Funds_ReadyToPrint_TC_012">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReadyToPrint_TC_012" />
		</classes>
	</test>
	
	<!--Verifying Ready To Print Functionality For Reset Printed Payment-->
	<test name="RMA_Reg_Funds_ReadyToPrint_TC013">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReadyToPrint_TC_013" />
		</classes>
	</test>

	<!--Verify that at the time of printing the pre-check batch there is no difference in financial details of the transaction from before printing-->
	<test name="RMA_Reg_Funds_ReadyToPrint_TC014">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReadyToPrint_TC_014" />
		</classes>
	</test>

	<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
</suite>