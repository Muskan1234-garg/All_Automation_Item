<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Suite10_Maint_Staff_Regression_01" />
	<!-- Enable Pagging Grid Configuration For Execution Of This Suite -->
	<!-- Test Case RMA_QTPRegression_TC_029_C Is Required To Reset The OLD UI 
		Settings After The Execution Of RMA_QTPRegression_TC_027 & RMA_QTPRegression_TC_029 -->

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!-- Verification Of Saving And Deleting Staff On Staff Maintenance Page -->
	<test name="RMA_Reg_Maint_Staff_TC_001">
