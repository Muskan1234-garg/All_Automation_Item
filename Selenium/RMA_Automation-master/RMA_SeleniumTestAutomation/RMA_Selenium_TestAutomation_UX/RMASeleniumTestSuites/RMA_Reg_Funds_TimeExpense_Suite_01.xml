<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!-- Change The System Time To Eastern Standard Time Before Execution Of 
		This Suite -->
	<!-- RMA_Reg_Funds_TimeExpense_TC_006 has some challanges to look at. RMA-96790 raised for the. Hence it is commented -->

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName"
		value="RMA_Reg_Funds_TimeExpense_Suite_01_CorporateGC" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!-- Verify User Is Able To Create, Edit And Delete Rate Table From Time 
		And Expense Rate Table Screen On Utilities -->
	<test name="RMA_Reg_Funds_TimeExpense_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_TimeExpense_TC_001" />
		</classes>
	</test>

	<!-- Successful Corporate General Claim Creation On Providing Mandatory 
		Data, Existence of Necessary Tabs With Data Entry Of One Text Field In Each, 
		Edit Of Created Claim Is Validated -->
	<test name="RMA_Smoke_TC_002_GC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!-- Verify User Is Able To Create Billable And Postable Transaction On 
		Time And Expense PopUp -->
	<test name="RMA_Reg_Funds_TimeExpense_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_TimeExpense_TC_002" />
		</classes>
	</test>

	<!-- Verify SMS Settings For Time And Expense -->
	<test name="RMA_Reg_Funds_TimeExpense_TC_007">
		<parameter name="LineOfBusiness" value="General Claims" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_TimeExpense_TC_007" />
		</classes>
	</test>

	<!-- Verify User Is Able To Create NonBillable And Postable Transaction 
		On Time And Expense PopUp -->
	<test name="RMA_Reg_Funds_TimeExpense_TC_003">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_TimeExpense_TC_003" />
		</classes>
	</test>

	<!-- Verify User Is Able To Create NonBillable And NonPostable Transaction 
		On Time And Expense PopUp -->
	<test name="RMA_Reg_Funds_TimeExpense_TC_004">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_TimeExpense_TC_004" />
		</classes>
	</test>

	<!-- Verify User Is Able To Create Non-Allocated Transaction On Time And 
		Expense PopUp -->
	<test name="RMA_Reg_Funds_TimeExpense_TC_005">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_TimeExpense_TC_005" />
		</classes>
	</test>

	<!-- Verify Claim Rate Table Is Not Visible On Claims Having Different Department
	<test name="RMA_Reg_Funds_TimeExpense_TC_006">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_TimeExpense_TC_006" />
		</classes>
	</test> -->

	<!-- Successful Corporate General Claim Creation On Providing Mandatory 
		Data, Existence of Necessary Tabs With Data Entry Of One Text Field In Each, 
		Edit Of Created Claim Is Validated -->
	<test name="RMA_Smoke_TC_002_GC_1">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!-- Verify User Is Able To Time And Expense Transaction When Insufficient 
		Funds Settings Is ON -->
	<test name="RMA_Reg_Funds_TimeExpense_TC_008">
		<parameter name="LineOfBusiness" value="General Claims" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_TimeExpense_TC_008" />
		</classes>
	</test>

	<!-- Verify User Is Able To Time And Expense Transaction When Auto Close 
		Option Is ON And Claim Is Getting Close After Billing Transaction -->
	<test name="RMA_Reg_Funds_TimeExpense_TC_009">
		<parameter name="LineOfBusiness" value="General Claims" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_TimeExpense_TC_009" />
		</classes>
	</test>

	<!-- Successful Corporate General Claim Creation On Providing Mandatory 
		Data, Existence of Necessary Tabs With Data Entry Of One Text Field In Each, 
		Edit Of Created Claim Is Validated -->
	<test name="RMA_Smoke_TC_002_GC_2">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!-- Verify User Is Able To Time And Expense Transaction On A Sub-Bank Account -->
	<test name="RMA_Reg_Funds_TimeExpense_TC_010">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_TimeExpense_TC_010" />
		</classes>
	</test>

	<!-- Successful Corporate General Claim Creation On Providing Mandatory 
		Data, Existence of Necessary Tabs With Data Entry Of One Text Field In Each, 
		Edit Of Created Claim Is Validated -->
	<test name="RMA_Smoke_TC_002_GC_3">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!-- Verify User Is Able To Print TnE Transaction Through Funds=>Print Checks 
		And TnE Txns Are Not Possible On Recovery Reserves -->
	<test name="RMA_Reg_Funds_TimeExpense_TC_011">
	<parameter name="LineOfBusiness" value="General Claims" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_TimeExpense_TC_011" />
		</classes>
	</test>
	<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>