<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!-- Note To Execute This Suite -->
	<!-- Set Base Currency To AUD -->
	<!-- Uncheck Setting 'Auto Adjust Negative Reserves To Zero' On Reserve 
		Settings On LOB -->

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Reg_PAYG_1" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required"RMASeleniumTestSuites/RMA_Maint_Physican_ERON.xml" 
		DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!-- PAYG Setup -->
	<test name="RMA_Regression_PAYG_SetUp">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_PAYG_SetUp" />
		</classes>
	</test>

	<!-- Employee,Non-Occ Disability Plan And Plan Class Creation With PAYG 
		Fields -->
	<test name="RMA_Regression_PAYG_01">
		<parameter name="TFNProvided" value="Yes" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_PAYG_01" />
		</classes>
	</test>

	<!--============================PAYG with TFN============================================ -->

	<!-- Non-Occ Payment Creation and Verification Of Calculated PAYG Payments,HELP/SSL/TSL 
		Payments and SFSS Payments -->
	<test name="RMA_Regression_PAYG_02">
		<parameter name="TFN" value="Yes" />
		<parameter name="TaxFreeThreshold" value="Yes" />
		<parameter name="Resident" value="Yes" />
		<parameter name="MedicareLevyExemption" value="None" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_PAYG_02" />
		</classes>
	</test>

	<!-- Non-Occ Payment Creation and Verification Of Calculated PAYG Payments,HELP/SSL/TSL 
		Payments and SFSS Payments -->
	<test name="RMA_Regression_PAYG_02_1">
		<parameter name="TFN" value="Yes" />
		<parameter name="TaxFreeThreshold" value="Yes" />
		<parameter name="Resident" value="No" />
		<parameter name="MedicareLevyExemption" value="None" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_PAYG_02" />
		</classes>
	</test>

	<!-- Non-Occ Payment Creation and Verification Of Calculated PAYG Payments,HELP/SSL/TSL 
		Payments and SFSS Payments -->
	<test name="RMA_Regression_PAYG_02_2">
		<parameter name="TFN" value="Yes" />
		<parameter name="TaxFreeThreshold" value="No" />
		<parameter name="Resident" value="Yes" />
		<parameter name="MedicareLevyExemption" value="None" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_PAYG_02" />
		</classes>
	</test>

	<!-- Non-Occ Payment Creation and Verification Of Calculated PAYG Payments,HELP/SSL/TSL 
		Payments and SFSS Payments -->
	<test name="RMA_Regression_PAYG_02_3">
		<parameter name="TFN" value="Yes" />
		<parameter name="TaxFreeThreshold" value="No" />
		<parameter name="Resident" value="No" />
		<parameter name="MedicareLevyExemption" value="None" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_PAYG_02" />
		</classes>
	</test>

	<!-- Non-Occ Payment Creation and Verification Of Calculated PAYG Payments,HELP/SSL/TSL 
		Payments and SFSS Payments -->
	<test name="RMA_Regression_PAYG_02_4">
		<parameter name="TFN" value="Yes" />
		<parameter name="TaxFreeThreshold" value="Yes" />
		<parameter name="Resident" value="Yes" />
		<parameter name="MedicareLevyExemption" value="Half" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_PAYG_02" />
		</classes>
	</test>

	<!-- Non-Occ Payment Creation and Verification Of Calculated PAYG Payments,HELP/SSL/TSL 
		Payments and SFSS Payments -->
	<test name="RMA_Regression_PAYG_02_5">
		<parameter name="TFN" value="Yes" />
		<parameter name="TaxFreeThreshold" value="Yes" />
		<parameter name="Resident" value="Yes" />
		<parameter name="MedicareLevyExemption" value="Full" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_PAYG_02" />
		</classes>
	</test>

<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>