
<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_DailySmoke_SupervisoryApproval1" />
	<parameter name="DataSourceRow" value="1" /><!--To Set The Datasource 
		Row Name From The sheet RMA_TC_002 of File RMASeleniumAutomationTestData -->

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--Successful New Users For Supervisory Chain Creation Is Validated Validated -->
	<!--To Set The No. Of Users in supervisory chain -->
	<parameter name="NoOfUsers" value="3" />
	<test name="RMA_Reg_Super_UserCreation_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_UserCreation_TC_001" />
		</classes>
	</test>

	<test name="RMA_Reg_Super_DeSelectALLSetting_TC_001">
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Super_DeSelectALLSetting_TC_001" />
		</classes>
	</test>

	<parameter name="LOB" value="GC - General Claims" />
	<parameter name="LimitFactor" value="10" />
	<test name="RMA_Reg_Super_ReserveLimit_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_001" />
		</classes>
	</test>

	<!--Successful Corporate Worker Claim Creation On Providing Mandatory Data, 
		Existence of Necessary Tabs With Edit Of One Text Field In Each, Edit Of 
		Created Claim Is Validated -->
	<test name="RMA_Smoke_TC_002_GC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>


	<!--Successful Reserve Can Be Created And Is Going On Hold Because Of Exceeded 
		Reserve Limits Is Validated -->
	<test name="RMA_Reg_Super_ReserveLimit_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_002" />
		</classes>
	</test>

	<!--Successful Verify That The Manager Can Have The Authority To Either 
		Approve Or Not Approve The Reserve Based On Reserve Limit Is Validated -->
	<test name="RMA_Reg_Super_ReserveLimit_TC_003">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_003" />
		</classes>
	</test>

	<!--Successful Verify Any User In Managerial Chain, Who Can See Reserve 
		In Its Supervisory Approval View Can Reject the Reserve Irrespective Of Its 
		Limit Is Validated -->
	<test name="RMA_Reg_Super_ReserveLimit_TC_004">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_004" />
		</classes>
	</test>s	

	<test name="RMA_Reg_Super_DeSelectALLSetting_TC_001_1">
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Super_DeSelectALLSetting_TC_001" />
		</classes>
	</test>

	<!--Successful Set Payment Limits At User And Validate It On Supervisory Chain -->
	<!--To Set The LOB Based on claim -->
	<parameter name="LOB" value="GC - General Claims" />
	<parameter name="LimitFactor" value="10" />
	<test name="RMA_Reg_Super_PaymentLimit_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_PaymentLimit_TC_001" />
		</classes>
	</test>

	<!--Successful Corporate General Claim Creation On Providing Mandatory Data,Existence 
		of Necessary Tabs With Edit Of One Text Field In Each, Edit Of Created Claim 
		Is Validated -->
	<test name="RMA_Smoke_TC_002_GC_01">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>


	<!-- Created Payment On Hold When Payment Limits Exceed -->
	<test name="RMA_Reg_Super_PaymentLimit_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_PaymentLimit_TC_002" />
		</classes>
	</test>

	<!--Manager's Authority To Either Able To Approve Or Not The Payment Based On Payment Limit -->
	<test name="RMA_Reg_Super_PaymentLimit_TC_003">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_PaymentLimit_TC_003" />
		</classes>
	</test>	
	
	<test name="RMA_Reg_Super_UserDeletion_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_UserDeletion_TC_001" />
		</classes>
	</test>

<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
</suite>