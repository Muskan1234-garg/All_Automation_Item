<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!-- Change The System Time To Eastern Standard Time Before Execution Of 
		This Suite -->

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Reg_AutoChecks_01" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--Successful Corporate General Claim Creation On Providing Mandatory Data, 
		Existence of Necessary Tabs With Edit Of One Text Field In Each, Edit Of 
		Created Claim Is Validated -->
	<test name="RMA_Smoke_TC_002_GC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<test name="RMA_Reg_Funds_AutoChecks_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_AutoChecks_TC_001" />
		</classes>
	</test>

	<test name="RMA_Reg_Funds_AutoChecks_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_AutoChecks_TC_002" />
		</classes>
	</test>

	<test name="RMA_Reg_Funds_AutoChecks_TC_003">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_AutoChecks_TC_003" />
		</classes>
	</test>

	<test name="RMA_Reg_Funds_AutoChecks_TC_004">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_AutoChecks_TC_004" />
		</classes>
	</test>

	<!--Reserve WorkSheet Settings For GC From Line Of Business Setup And Reserve 
		WorkSheet Configuration Setting From Utilities Is Validated -->
	<test name="RMA_Reg_Funds_ReserveWorkSheet_Settings">
		<parameter name="LOB" value="GC - General Claims" />
		<parameter name="numofreserves" value="2" />
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorkSheet_Settings" />
		</classes>
	</test>

	<!-- Corporate General Claim Creation In RMA Application -->
	<test name="RMA_Smoke_TC_002_GC_2">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!-- Creation of AutoChecks On Reserve WorkSheet And Stop Pay Operation 
		On It Is Validated -->
	<test name="RMA_Reg_Funds_AutoChecks_TC_021">
		<parameter name="LOB" value="GC" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_AutoChecks_TC_021" />
		</classes>
	</test>

	<!-- Add Person Involved To Payor Type Setting On Funds Settings Tab On 
		General System Parameters Screen And Unclear Of Auto Check Is Validated -->
	<test name="RMA_Reg_Funds_AutoChecks_TC_022">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_AutoChecks_TC_022" />
		</classes>
	</test>

	<!-- Revert Reserve WorkSheet Settings From Line Of Business Setup -->
	<test name="RMA_Reg_Funds_ReserveWorkSheet_RevertSettings">
		<parameter name="LOB" value="GC - General Claims" />
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorkSheet_RevertSettings" />
		</classes>
	</test>

	<!--Successful Corporate General Claim Creation On Providing Mandatory Data, 
		Existence of Necessary Tabs With Edit Of One Text Field In Each, Edit Of 
		Created Claim Is Validated -->
	<test name="RMA_Smoke_TC_002_GC_Smoke_2">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>


	<!-- Successful Reserve Creation On Claim Is Validated -->
	<test name="RMA_Smoke_TC_009">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_009" />
		</classes>
	</test>

	<!-- Check Void Reason Setting On Payment Parameters Setup Screen Is Validated 
		For AutoChecks -->
	<test name="RMA_Reg_Funds_AutoChecks_TC_025">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_AutoChecks_TC_025" />
		</classes>
	</test>

	<!-- Default Distribution Type On AutoChecks When Manual is Selected From 
		Payment Parameter Screen Is Validated -->
	<test name="RMA_Reg_Funds_AutoChecks_TC_026">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_AutoChecks_TC_026" />
		</classes>
	</test>

	<!--Verify That User Can Logged out From Application -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
</suite>