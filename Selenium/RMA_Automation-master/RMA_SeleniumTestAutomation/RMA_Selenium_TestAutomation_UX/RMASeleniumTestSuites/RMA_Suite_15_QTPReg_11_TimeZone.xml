<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="5" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Suite_15_QTPReg_11_TimeZone"/>

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--  Date/Time Validations For General Claims When Time Zone Is Enabled For Org Level: Client & Time Zone Is AL -->
	<test name="RMA_QTPRegression_TC_032">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_032" />
		</classes>
	</test>
	
	<!--  Date/Time Validations For General Claims When Time Zone Is Enabled For Org Level: Client & Time Zone Is CST -->
	<test name="RMA_QTPRegression_TC_033">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_033" />
		</classes>
	</test>
	
	<!--  Date/Time Validations For Worker's Compensation Claims When Time Zone Is Enabled For Org Level: Client & Time Zone Is AL -->
	<test name="RMA_QTPRegression_TC_034">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_034" />
		</classes>
	</test>
	
	<!--  Date/Time Validations For Worker's Compensation Claims When Time Zone Is Enabled For Org Level: Client & Time Zone Is CST -->
	<test name="RMA_QTPRegression_TC_044">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_044" />
		</classes>
	</test>
	
	<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>

