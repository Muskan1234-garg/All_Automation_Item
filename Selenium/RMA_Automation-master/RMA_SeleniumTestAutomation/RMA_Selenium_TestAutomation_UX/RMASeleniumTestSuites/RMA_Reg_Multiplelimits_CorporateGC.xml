<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!-- Change The System Time To Eastern Standard Time Before Execution Of 
		This Suite -->

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Reg_Multiplelimits_CorporateGC" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!-- Verify the SMS Settings For Payment/Reserve Tab On SuperVisory Approval 
		Screen -->
	<test name="RMA_Reg_Super_MultipleLimit_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_MultipleLimit_TC_001" />
		</classes>
	</test>

	<!--Successful New Users For Supervisory Chain Creation Is Validated Validated -->
	<!--To Set The No. Of Users in supervisory chain -->
	<parameter name="NoOfUsers" value="3" />
	<test name="RMA_Reg_Super_UserCreation_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_UserCreation_TC_001" />
		</classes>
	</test>

	<!--Successful Revert Setting From Utilities Page Is Validated -->
	<test name="RMA_Reg_Super_DeSelectALLSetting_TC_001">
		<parameter name="LOB" value="GC - General Claims" />
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Super_DeSelectALLSetting_TC_001" />
		</classes>
	</test>

	<!--Successful Set Payment Limits At User And Validate It On Supervisory 
		Chain -->
	<!--To Set The LOB Based on claim -->
	<parameter name="LOB" value="GC - General Claims" />
	<test name="RMA_Reg_Super_PaymentLimit_TC_001">
		<parameter name="LimitFactor" value="10" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_PaymentLimit_TC_001" />
		</classes>
	</test>

	<!--Successful Set Reserve Limits At User And Validate It On Supervisory 
		Chain -->
	<!--To Set The LOB Based on claim -->
	<parameter name="LOB" value="GC - General Claims" />
	<test name="RMA_Reg_Super_ReserveLimit_TC_001">
		<parameter name="LimitFactor" value="10" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_001" />
		</classes>
	</test>

	<!--Successful Corporate General Claim Creation On Providing Mandatory Data,Existence 
		of Necessary Tabs With Edit Of One Text Field In Each, Edit Of Created Claim 
		Is Validated -->
	<test name="RMA_Smoke_TC_002_GC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!-- Verify 3 Reserves And 3 Payments Can Be Created And Is Going On Hold 
		Because Of Exceeded Reserve Limits And Exceeded Payment Limits Are Validated -->
	<test name="RMA_Reg_Super_MultipleLimit_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_MultipleLimit_TC_002" />
		</classes>
	</test>

	<!-- Verify That Payments And Reserves Are Visible Only To Immediate Manager 
		And Have Authority To Approve And Reject Is Validated -->
	<test name="RMA_Reg_Super_MultipleLimit_TC_003">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_MultipleLimit_TC_003" />
		</classes>
	</test>

	<!--Successful Revert Setting From Utilities Page Is Validated -->
	<test name="RMA_Reg_Super_DeSelectALLSetting_TC_001_1">
		<parameter name="LOB" value="GC - General Claims" />
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Super_DeSelectALLSetting_TC_001" />
		</classes>
	</test>

	<!--Successful Set Payment Limits At User And Validate It On Supervisory 
		Chain -->
	<parameter name="LOB" value="GC - General Claims" />
	<test name="RMA_Reg_Super_PaymentLimit_TC_001_1">
		<parameter name="LimitFactor" value="10" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_PaymentLimit_TC_001" />
		</classes>
	</test>

