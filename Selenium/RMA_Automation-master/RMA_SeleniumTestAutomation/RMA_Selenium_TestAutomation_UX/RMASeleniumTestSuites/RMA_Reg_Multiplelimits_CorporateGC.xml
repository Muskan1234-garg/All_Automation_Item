	<parameter name="LOB" value="GC - General Claims" />
	<test name="RMA_Reg_Super_PayDetailLimit_TC_001">
		<parameter name="LimitFactor" value="20" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_PayDetailLimit_TC_001" />
		</classes>
	</test>

	<!--Successful Corporate General Claim Creation On Providing Mandatory Data,Existence 
		of Necessary Tabs With Edit Of One Text Field In Each, Edit Of Created Claim 
		Is Validated -->
	<test name="RMA_Smoke_TC_002_GC_1">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!-- Verify Payment Limit And Pay Details Limit On Payments And Approve/Reject 
		By Manager Is Validated -->
	<test name="RMA_Reg_Super_MultipleLimit_TC_004">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_MultipleLimit_TC_004" />
		</classes>
	</test>

	<!--Successful Revert Setting From Utilities Page Is Validated -->
	<test name="RMA_Reg_Super_DeSelectALLSetting_TC_001_2">
		<parameter name="LOB" value="GC - General Claims" />
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Super_DeSelectALLSetting_TC_001" />
		</classes>
	</test>

	<!--Successful Set Payment Limits At User And Validate It On Supervisory 
		Chain -->
	<!--To Set The LOB Based on claim -->
	<parameter name="LOB" value="GC - General Claims" />
	<test name="RMA_Reg_Super_PaymentLimit_TC_001_2">
		<parameter name="LimitFactor" value="10" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_PaymentLimit_TC_001" />
		</classes>
	</test>

	<!--Successful Set Reserve Limits At User And Validate It On Supervisory 
		Chain -->
	<!--To Set The LOB Based on claim -->
	<parameter name="LOB" value="GC - General Claims" />
	<test name="RMA_Reg_Super_ReserveLimit_TC_001_1">
		<parameter name="LimitFactor" value="100" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_ReserveLimit_TC_001" />
		</classes>
	</test>

	<!--Successful Set Pay Detail Limits At User And Validate It On Supervisory 
		Chain -->
	<!--To Set The LOB Based on claim -->
	<parameter name="LOB" value="GC - General Claims" />
	<test name="RMA_Reg_Super_PayDetailLimit_TC_001_1">
		<parameter name="LimitFactor" value="20" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_PayDetailLimit_TC_001" />
		</classes>
	</test>

	<!--Successful Set Per Claim Incurred Limits At User And Validate It On 
		Supervisory Chain -->
	<!--To Set The LOB Based on claim -->
	<test name="RMA_Reg_Super_PerClaimIncurredLimit_TC_001">
		<parameter name="LOB" value="GC - General Claims" />
		<parameter name="LimitFactor" value="300" />
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Super_PerClaimIncurredLimit_TC_001" />
		</classes>
	</test>

	<!--Successful Corporate General Claim Creation On Providing Mandatory Data,Existence 
		of Necessary Tabs With Edit Of One Text Field In Each, Edit Of Created Claim 
		Is Validated -->
	<test name="RMA_Smoke_TC_002_GC_2">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!-- Verify 3 Reserves And 3 Payments Can Be Created And Is Going On Hold 
		Because Of Exceeded Reserve Limits, Exceeded Payment Limits, Exceeded Per 
		Claim Incurred Limit And Exceeded Payment Detail Limit Are Validated -->
	<test name="RMA_Reg_Super_MultipleLimit_TC_005">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_MultipleLimit_TC_005" />
		</classes>
	</test>

	<!--Successful Revert Setting From Utilities Page Is Validated -->
	<test name="RMA_Reg_Super_DeSelectALLSetting_TC_001_3">
		<parameter name="LOB" value="GC - General Claims" />
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Super_DeSelectALLSetting_TC_001" />
		</classes>
	</test>
<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>

