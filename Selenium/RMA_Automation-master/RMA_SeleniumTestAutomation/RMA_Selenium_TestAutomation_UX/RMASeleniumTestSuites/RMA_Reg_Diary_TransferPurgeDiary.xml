<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!-- Change The System Time To Eastern Standard Time Before Execution Of 
		This Suite -->

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Reg_DiaryUtilities_TransferPurgeDiary" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--To Create New Users -->
	<parameter name="NoOfUsers" value="2" />
	<test name="RMA_Reg_Super_UserCreation_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Super_UserCreation_TC_001" />
		</classes>
	</test>

	<!-- Transfer Of Open Diaries And Closed Diaries -->
	<test name="RMA_Regression_Diary_TC_20">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_Diary_TC_20" />
		</classes>
	</test>

	<!-- Purge Of Open Diaries And Closed Diaries -->
	<test name="RMA_Regression_Diary_TC_21">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_Diary_TC_21" />
		</classes>
	</test>

	<!-- Verify When Assigned User Edits The Diary, Assigned User And Assigning 
		User Do Not Change -->
	<test name="RMA_Regression_Diary_TC_25">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_Diary_TC_25" />
		</classes>
	</test>

	<!-- Create Diary Search Form Query Designer -->
	<test name="RMA_Regression_Diary_TC_26">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Regression_Diary_TC_26" />
		</classes>
	</test>
<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
</suite>