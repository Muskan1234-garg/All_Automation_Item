			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_DisabilityPlan_TC_010" />
		</classes>
	</test>

	<!--Various Fields, Tabs And Creation Of Plan Class(Weekly) With Mandatory Fields On Disability Plan Management Are Validated -->
