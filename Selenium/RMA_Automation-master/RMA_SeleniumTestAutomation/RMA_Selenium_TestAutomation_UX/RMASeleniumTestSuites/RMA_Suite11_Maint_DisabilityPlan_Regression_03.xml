<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Suite11_Maint_DisabilityPlan_Regression_03" />
	<!-- Create a Disability Plan Mail Merge Template With Name "DisabilityPlan_Template" For Test Case RMA_Reg_Maint_DisabilityPlan_TC_005 -->

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--Verify Maintenance Screen Navigation In RMA Application -->
	<test name="RMA_Smoke_TC_041_I">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_041_I" />
		</classes>
	</test>

	<!--Verify The Display Of Some Fields On Selection Of Value In Other Fields -->
	<test name="RMA_Reg_Maint_DisabilityPlan_TC_008">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_DisabilityPlan_TC_008" />
		</classes>
	</test>
	
	<!--Verify User Is Able To Select Department Or Any Level From The OrgHierachy As Insured(s)/Account(s) -->
