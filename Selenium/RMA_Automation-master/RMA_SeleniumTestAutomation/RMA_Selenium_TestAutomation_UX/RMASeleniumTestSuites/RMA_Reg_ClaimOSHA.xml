<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
<parameter name="DataSourceRow" value="2" />

<!--To Set The Executed SuiteName-->
<parameter name="ExecutedSuiteName" value="RMA_Reg_Suite_Claim_OSHA" />

<!--Verify That RMA Application Is Logged In-->
  <test name="RMA_TC_001">
    <classes>
      <class name="rmaseleniumtestscripts.RMA_TC_001"/>
    </classes>
  </test>
  
<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>
<!-- Verify Maintenance Screen Navigation In RMA Application -->
	<test name="RMA_Smoke_TC_041_I">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_041_I" />
		</classes>
	</test>

<!-- Verify OSHA Information For PI At Claim Level In RMA Application -->
	
	<test name="RMA_Claim_OSHA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Claim_OSHA_TC_001" />
		</classes>
	</test>	
<!-- Verify Post OSHA Report In RMA Application -->
	
	<test name="RMA_Claim_OSHA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Claim_OSHA_TC_002" />
		</classes>
	</test>
			
	<test name="RMA_Claim_OSHA_TC_003">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Claim_OSHA_TC_003" />
		</classes>
	</test>		
<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
</suite>