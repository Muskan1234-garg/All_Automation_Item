	<!--Verify That User Is Able To Add/Delete Fields In PowerViewEditor For 
		Employee Screen and User Is Able To Save New Employee -->
	<test name="RMA_Reg_Maint_Employee_TC_023">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Employee_TC_023" />
		</classes>
	</test>

	<!--Verify That Same Fields Are Coming In PowerView As Defined In PowerView 
		Editor For Employee Screen -->
	<test name="RMA_Reg_Maint_Employee_TC_022">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Employee_TC_022" />
		</classes>
	</test>

	<!--Verify That User Is Able To Add New Supplemental Field On Dependents -->
	<test name="RMA_Reg_Maint_Employee_TC_021">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Employee_TC_021" />
		</classes>
	</test>

	<!--Verify That Error Must Be Displayed If Required Supplemental Field Is 
		Not Present On The Dependent -->
	<test name="RMA_Reg_Maint_Employee_TC_020">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Employee_TC_020" />
		</classes>
	</test>

	<!--Verify That Error Must Be Displayed If Required Supplemental Field Is 
		Not Present On The Employee -->
	<test name="RMA_Reg_Maint_Employee_TC_019">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Employee_TC_019" />
		</classes>
	</test>

	<!--Verify That When User Changes The ToolTip Of Any ToolBar Button Then 
		The Same Is Updated In UX On Employee Page -->
	<test name="RMA_Reg_Maint_Employee_TC_035">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Employee_TC_035" />
		</classes>
	</test>

	<!--Verify That When User Changes The ToolTip Of Navigation Link By UX Power 
		View Then Same Is Updated Correctly -->
	<test name="RMA_Reg_Maint_Employee_TC_034">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Employee_TC_034" />
		</classes>
	</test>

	<!--Verify That Field Becomes ReadOnly When User Enables Read Only Flag 
		Via UX PowerView On Employee Maintenance -->
	<test name="RMA_Reg_Maint_Employee_TC_037">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Employee_TC_037" />
		</classes>
	</test>

<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
