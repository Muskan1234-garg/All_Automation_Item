<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="2" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Reg_Suite_BRSPaymentOverRide" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--Successful Corporate General Claim Creation On Providing Mandatory Data, 
		Existence of Necessary Tabs With Edit Of One Text Field In Each, Edit Of 
		Created Claim Is Validated -->
	<test name="RMA_Smoke_TC_002_GC_Smoke_I">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC_Smoke" />
		</classes>
	</test>

	<!--: Successful Creation of BRS Payment with Clonned & Delete Features -->
	<test name="RMA_Reg_Funds_BRSPayments_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_BRSPayments_TC_002" />
		</classes>
	</test>

	<!--: Successful Creation of an Entity having Contract Details -->
	<test name="RMA_Reg_Funds_BRSContractEntity_TC_003">
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_BRSContractEntity_TC_003" />
		</classes>
	</test>

	<!--: Successful Creation of BRS Payment with Override Payment Option. Override 
		Option - Billed Amount -->
	<test name="RMA_Reg_Funds_BRSPayments_TC_014">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_BRSPayments_TC_014" />
		</classes>
	</test>

	<!--: Successful Creation of BRS Payment with Override Payment Option. Override 
		Option - Billed Amount With Discount -->
	<test name="RMA_Reg_Funds_BRSPayments_TC_008">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_BRSPayments_TC_008" />
		</classes>
	</test>

	<!--: Successful Creation of BRS Payment with Override Payment Option. Override 
		Option - Contract Amount -->
	<test name="RMA_Reg_Funds_BRSPayments_TC_012">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_BRSPayments_TC_012" />
		</classes>
	</test>

	<!--: Successful Creation of BRS Payment with Override Payment Option. Override 
		Option - Contract Amount With Discount -->
	<test name="RMA_Reg_Funds_BRSPayments_TC_013">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_BRSPayments_TC_013" />
		</classes>
	</test>

	<!--: Successful Creation of BRS Payment with Override Payment Option. Override 
		Option - Fee Schedule -->
	<test name="RMA_Reg_Funds_BRSPayments_TC_010">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_BRSPayments_TC_010" />
		</classes>
	</test>

	<!--: Successful Creation of BRS Payment with Override Payment Option. Override 
		Option - Fee Schedule With Discount -->
	<test name="RMA_Reg_Funds_BRSPayments_TC_011">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_BRSPayments_TC_011" />
		</classes>
	</test>

	<!--: Successful Creation of BRS Payment with Override Payment Option. Override 
		Option - Apply Per Diem -->
	<test name="RMA_Reg_Funds_BRSPayments_TC_009">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_BRSPayments_TC_009" />
		</classes>
	</test>

	<!--: Successful Creation of BRS Payment with Override Payment Option. Override 
		Option - Apply Per Diem With Stop Loss -->
	<test name="RMA_Reg_Funds_BRSPayments_TC_006">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_BRSPayments_TC_006" />
		</classes>
	</test>

	<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>

</suite>