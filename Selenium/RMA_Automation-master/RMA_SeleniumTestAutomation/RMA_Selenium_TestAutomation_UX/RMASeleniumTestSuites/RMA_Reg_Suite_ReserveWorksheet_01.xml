		</classes>
	</test>

	<!-- Successful Carrier WC Claim Creation Is Validated -->
	<test name="RMA_Smoke_TC_075">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_075" />
		</classes>
	</test>

	<!-- To Save a Reserve WorkSheet On A Carrier Worker's Claim -->
	<test name="RMA_Reg_Funds_ReserveWorkSheet_TC_009">
		<parameter name="LOB" value="WC" />
		<parameter name="PolicyType" value="External" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorkSheet_TC_009"></class>
		</classes>
	</test>

	<!--Verify that if WorkSheet Approval Setting Is ON From SMS Then It Should 
		Get Displayed In Supervisory Approval -->
	<test name="RMA_Reg_Funds_ReserveWorkSheet_TC_007">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorkSheet_TC_007"></class>
		</classes>
	</test>

	<!-- Verify That Corporate Reserve WorkSheet Should Work With Corporate 
		Internal Policy Is Validated -->
	<test name="RMA_Reg_Funds_ReserveWorkSheet_TC_019">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorkSheet_TC_019" />
		</classes>
	</test>

	<!-- Revert Reserve WorkSheet Settings From Line Of Business Setup -->
	<test name="RMA_Reg_Funds_ReserveWorkSheet_RevertSettings">
		<parameter name="LOB" value="GC - General Claims" />
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorkSheet_RevertSettings" />
		</classes>
	</test>

	<!-- Revert Reserve WorkSheet Settings From Line Of Business Setup -->
