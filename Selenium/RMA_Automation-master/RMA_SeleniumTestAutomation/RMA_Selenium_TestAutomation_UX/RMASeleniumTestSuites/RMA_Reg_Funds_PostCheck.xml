<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Regs_PostChecks" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>
	
    <!--Successful Corporate General Claim Creation On Providing Mandatory Data, Existence of Necessary Tabs With Data Entry Of One Text Field In Each, Edit Of Created Claim Is Validated -->
	<test name="RMA_Smoke_TC_002_GC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test> 
	
    <!-- Successful Reserve Creation, Reserve Grid Value Verification, Color Code Change And Tool Tip Amount Of Reserve Progress Image On Claim Screen Is Validated -->
	<test name="RMA_Smoke_TC_009">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_009" />
		</classes>
	</test>

	<test name="RMA_Reg_PrintChecks_TC_14">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_PrintChecks_TC_14" />
		</classes>
	</test>
	
	<test name="RMA_Reg_PrintChecks_TC_17">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_PrintChecks_TC_17" />
		</classes>
	</test>

	<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>