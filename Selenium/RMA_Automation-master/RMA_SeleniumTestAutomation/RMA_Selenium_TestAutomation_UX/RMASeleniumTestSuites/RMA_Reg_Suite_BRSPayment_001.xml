<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!-- Change The System Time To Eastern Standard Time Before Execution Of 
		This Suite -->

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Reg_Suite_BRSPayment_001" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--Creation Of GC With Values In All Mandatory Fields -->
	<test name="RMA_Smoke_TC_002_GC_Smoke_I">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!--Verification Of Mandatory Fields On BRS Screen And Creation Of BRS Payment 
		Are Validated -->
	<test name="RMA_Reg_Funds_BRSPayments_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_BRSPayments_TC_001" />
		</classes>
	</test>

	<!--Printing, Clearing & Voiding Of BRS Payments Are Validated -->
	<test name="RMA_Reg_Funds_BRSPayments_TC_004">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_BRSPayments_TC_004" />
		</classes>
	</test>

	<!--Printing Of BRS Payments Through Task Manager Is Validated -->
	<test name="RMA_Reg_Funds_BRSPayments_TC_005">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_BRSPayments_TC_005" />
		</classes>
	</test>

	<!--EOB Settings On Payment Parameters Setup Screen Are Validated -->
	<test name="RMA_Reg_Funds_BRSPayments_TC_007">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_BRSPayments_TC_007" />
		</classes>
	</test>

	<!--Verify That User Can Logged out From Application -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>

</suite>