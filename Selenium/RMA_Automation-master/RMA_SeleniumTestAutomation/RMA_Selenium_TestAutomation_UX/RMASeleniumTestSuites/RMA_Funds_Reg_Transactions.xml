<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!-- Change The System Time To Eastern Standard Time Before Execution Of 
		This Suite -->

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName"
		value="RMA_Funds_Reg_Transactions" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>
	
	<!--Verify Maintenance Screen Navigation In RMA Application -->
	<test name="RMA_Smoke_TC_041_I_1">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_041_I" />
		</classes>
	</test>		
	
	<!-- Payees Are Created For Transactions As Per The Parameter NoOfPayees--> 
	<test name="RMA_Reg_Funds_Transactions_PayeeCreation_1">
	<parameter name="NoOfPayees" value="2" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_Transactions_PayeeCreation" />
		</classes>
	</test>		
	
	<!-- Verify Print Preview Of Orphan Transaction And Verify Printing Of Orphan Transaction From Transaction Screen -->
	 <test name="RMA_Reg_Funds_Transactions_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_Transactions_TC_001" />
		</classes>
	</test>	 
	
	<!-- Verify Clone Of Printed Check On Transaction Screen -->
	<test name="RMA_Reg_Funds_Transactions_TC_003">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_Transactions_TC_003" />
		</classes>
	</test>
	
	<!-- Verify Print Summary On Transaction Screen -->
	 <test name="RMA_Reg_Funds_Transactions_TC_004">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_Transactions_TC_004" />
		</classes>
	</test>	
	
	<!-- Verify Payee Experience On Transaction Screen -->
	<test name="RMA_Reg_Funds_Transactions_TC_005">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_Transactions_TC_005" />
		</classes>
	</test>	
	
	<!-- Printing Of Orphan Transaction From Funds - Print Checks -->
	<test name="RMA_Reg_Funds_Transactions_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_Transactions_TC_002" />
		</classes>
	</test>
	
	<!-- Verify Print Preview Of Orphan Transaction Created With Multiple Payee And Verify Its Printing From Transaction Screen -->
	<test name="RMA_Reg_Funds_Transactions_TC_006">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_Transactions_TC_006" />
		</classes>
	</test>

		<!--Successful Corporate General Claim Creation On Providing Mandatory Data,Existence 
		of Necessary Tabs With Edit Of One Text Field In Each, Edit Of Created Claim 
		Is Validated -->
	<test name="RMA_Smoke_TC_002_GC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>		
	
	<!-- Verify Print Preview Of Payment Created With Multiple Payee And Verify Its Printing From Funds-PrintChecks -->
	<test name="RMA_Reg_Funds_Transactions_TC_007">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_Transactions_TC_007" />
		</classes>
	</test>

	<!--Verify Maintenance Screen Navigation In RMA Application -->
	<test name="RMA_Smoke_TC_041_I">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_041_I" />
		</classes>
	</test>	
	
	<!-- Payees Are Created For Transactions As Per The Parameter NoOfPayees--> 
	<test name="RMA_Reg_Funds_Transactions_PayeeCreation">
	<parameter name="NoOfPayees" value="1" />
	<parameter name="IncludePrefixSuffix" value="Yes" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_Transactions_PayeeCreation" />
		</classes>
	</test>	
	
	<!-- Verify Include Prefix on Payments And Include Suffix on Payments Settings--> 
	<test name="RMA_Reg_Funds_Transactions_IncludePrefixSuffixSettings">
	<parameter name="IncludePrefixSuffix" value="check" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_Transactions_IncludePrefixSuffixSettings" />
		</classes>
	</test>			
	
	<!-- Verify Include Prefix on Payments And Include Suffix on Payments For Payments From Funds-Print Checks -->
	<test name="RMA_Reg_Funds_Transactions_TC_008">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_Transactions_TC_008" />
		</classes>
	</test>
	
	<!-- Verify Include Prefix on Payments And Include Suffix on Payments For Payments From Print Check Button -->
	<test name="RMA_Reg_Funds_Transactions_TC_009">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_Transactions_TC_009" />
		</classes>
	</test>
	
	<!-- Verify Include Prefix on Payments And Include Suffix on Payments Settings--> 
	<test name="RMA_Reg_Funds_Transactions_IncludePrefixSuffixSettings_uncheck">
	<parameter name="IncludePrefixSuffix" value="uncheck" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_Transactions_IncludePrefixSuffixSettings" />
		</classes>
	</test>	
	
	<!--Successful Corporate General Claim Creation On Providing Mandatory Data, Existence of Necessary Tabs With Edit Of One Text Field In Each, Edit Of Created Claim Is Validated -->
	 <test name="RMA_Smoke_TC_002_GC_Last">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>
	
	
 	<!-- Successful Reserve Creation Is Validated --> 
	<test name="RMA_Smoke_TC_009">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_009" />
		</classes>
	</test>
	
	<!-- Default Distribution Type On Payment Screen When Manual is Selected From Payment Parameter Screen Is Validated --> 
	<test name="RMA_Reg_Funds_Transactions_TC_010">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_Transactions_TC_010" />
		</classes>
	</test>	
	
	<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>

</suite>