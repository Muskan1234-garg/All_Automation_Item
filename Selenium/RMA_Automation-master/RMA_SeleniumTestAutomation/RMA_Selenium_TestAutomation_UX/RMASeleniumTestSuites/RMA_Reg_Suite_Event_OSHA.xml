<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
<parameter name="DataSourceRow" value="2" />

<!--To Set The Executed SuiteName-->
<parameter name="ExecutedSuiteName" value="RMA_Reg_Suite_Event_OSHA" />

<!--Verify That RMA Application Is Logged In-->
  <test name="RMA_TC_001">
    <classes>
      <class name="rmaseleniumtestscripts.RMA_TC_001"/>
    </classes>
  </test>
  
<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>
<!-- Verify Maintenance Screen Navigation In RMA Application -->
	<test name="RMA_Smoke_TC_041_I">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_041_I" />
		</classes>
	</test>

<!-- Successful OSHA Creation On Providing Mandatory Data Is Validated -->
	<test name="RMA_Event_OSHA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Event_OSHA_TC_001" />
		</classes>
	</test>

<!-- Successful Posting OSHA Report Is Validated --> 
	<test name="RMA_Event_OSHA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Event_OSHA_TC_002" />
		</classes>
	</test>	

<!-- Successful Running OSHA Report Is Validated -->
	<test name="RMA_Event_OSHA_TC_003">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Event_OSHA_TC_003" />
		</classes>
	</test>	
		
<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
</suite>