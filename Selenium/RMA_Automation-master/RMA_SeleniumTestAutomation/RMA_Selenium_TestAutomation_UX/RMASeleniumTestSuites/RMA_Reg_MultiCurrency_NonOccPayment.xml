<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Reg_MultiCurrency_NonOccPayment" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>	
	
	<test name="RMA_Reg_Globalization_TC01">
		<parameter name="BaseCurrency" value="EUR" />
		<parameter name="SourceCurrency" value="GBP" />
		<parameter name="DestinationCurrency" value="USD" />
		<parameter name="Claim_Base_Rate" value="1.18" />
		<parameter name="Base_Base_Rate" value="1" />
		<parameter name="Claim_Claim_Rate" value="1" />
		<parameter name="Claim_Policy_Rate" value="1.24" />
		<parameter name="Policy_Policy_Rate" value="1" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Globalization_TC01" />
		</classes>
	</test>
	
	<test name="RMA_Reg_Globalization_NonOccPayments_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Globalization_NonOccPayments_TC_001" />
		</classes>
	</test>

	<test name="RMA_Reg_Globalization_NonOccPayments_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Globalization_NonOccPayments_TC_002" />
		</classes>
	</test>
	
	<test name="RMA_Reg_Globalization_NonOccPayments_TC_003">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Globalization_NonOccPayments_TC_003" />
		</classes>
	</test>
	
	<test name="RMA_Reg_Globalization_NonOccPayments_TC_004">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Globalization_NonOccPayments_TC_004" />
		</classes>
	</test>


	<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>