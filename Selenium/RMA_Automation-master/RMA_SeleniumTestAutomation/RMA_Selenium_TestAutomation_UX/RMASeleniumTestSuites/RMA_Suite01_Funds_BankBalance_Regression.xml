<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Suite01_Funds_BankBalance_Regression" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--Creating Multiple Balance On A Bank Account And Verify Account Balance 
		are Validated -->
	<test name="RMA_Reg_Funds_BankAccBalance_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_BankAccBalance_TC_001" />
		</classes>
	</test>

	<!-- Upload Of File With All the Parameters And Verification Of PDF File 
		are Validated -->
	<test name="RMA_Reg_Funds_BankAccBalance_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_BankAccBalance_TC_002" />
		</classes>
	</test>

<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>