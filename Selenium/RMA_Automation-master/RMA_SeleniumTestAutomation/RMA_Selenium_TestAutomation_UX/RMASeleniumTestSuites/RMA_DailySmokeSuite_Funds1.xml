<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_DailySmokeSuite_Funds1" />

	<parameter name="DataSourceRow" value="1" /><!--To Set The Datasource Row Name From The sheet RMA_TC_002 of File RMASeleniumAutomationTestData -->


	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<parameter name="ZoneName" value="Dashboard" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--Successful Corporate General Claim Creation On Providing Mandatory Data,Existence 
		of Necessary Tabs With Edit Of One Text Field In Each, Edit Of Created Claim 
		Is Validated -->
	<test name="RMA_Smoke_TC_002_GC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!-- Successful Reserve Creation, Reserve Grid Value Verification, Color 
		Code Change And Tool Tip Amount Of Reserve Progress Image On GC Claim Screen 
		Is Validated -->
	<test name="RMA_Smoke_TC_009">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_009" />
		</classes>
	</test>

	<!--Creation Of Printed/Released Payments/Collection And Voided From Funds=>Void 
		Checks Is Validated -->
	<test name="RMA_Reg_Funds_VoidCheck_TC_003">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_VoidCheck_TC_003" />
		</classes>
	</test>

	<!--Various Filters Like Bank account, Company, From Date, To Date and Include 
		Collections On Void Checks Are Validated -->
	<test name="RMA_Reg_Funds_VoidCheck_TC_004">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_VoidCheck_TC_004" />
		</classes>
	</test>

	<!--After voiding Of Payment, Payment Amount Is Added Back To Reserve Balance 
		Is Validated -->
	<test name="RMA_Reg_Funds_VoidCheck_TC_005">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_VoidCheck_TC_005" />
		</classes>
	</test>

	<!--Setting Up A Bank account With Check Stock Is Validated -->
	<test name="RMA_Reg_Funds_BankAccount_TC_029">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_BankAccount_TC_029" />
		</classes>
	</test>

	<!--Successful Corporate General Claim Creation On Providing Mandatory Data,Existence 
		of Necessary Tabs With Edit Of One Text Field In Each, Edit Of Created Claim 
		Is Validated -->
<!-- 	<test name="RMA_Smoke_TC_002_GC_2"> -->
<!-- 		<classes> -->
<!-- 			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" /> -->
<!-- 		</classes> -->
<!-- 	</test> -->

	<!-- Printing Checks When Printing Option Is Set To Printer And Custom File 
		Only From Funds->Print Checks -->
	<test name="RMA_Reg_PrintChecks_TC_06">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_PrintChecks_TC_06" />
		</classes>
	</test>

	<!-- Printing Auto Checks From Funds>Prints Checks Is Validated -->
	<test name="RMA_Reg_PrintChecks_TC_10">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_PrintChecks_TC_10" />
		</classes>
	</test>

<!--Successful Corporate General Claim Creation On Providing Mandatory Data,Existence 
		of Necessary Tabs With Edit Of One Text Field In Each, Edit Of Created Claim 
		Is Validated -->
	<test name="RMA_Smoke_TC_002_GC_4">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!-- Payments Do Not Go On Hold When Pay Detail Limits Are Exceeded CheckBox 
		is OFF And Deselect All Settings -->
	<test name="RMA_Reg_Funds_AllocatedBucketForPayments_TC_001">
		<parameter name="LineOfBusiness" value="GC - General Claims" />
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_AllocatedBucketForPayments_TC_001" />
		</classes>
	</test>
	
	<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
</suite>