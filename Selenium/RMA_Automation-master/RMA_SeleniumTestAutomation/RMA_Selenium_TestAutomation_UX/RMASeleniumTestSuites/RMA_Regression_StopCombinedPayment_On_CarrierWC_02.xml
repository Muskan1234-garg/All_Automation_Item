<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Regression_StopCombinedPayment_On_CarrierWC_02" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>
	
 	<!--Combined Payment Setup In RMA Application -->
	<test name="RMA_QTPRegression_TC_052_I"> 
		<classes>
			<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_052_I" />
		</classes>
	</test>
	
	<!--Stop Combined Payment With "Stop Combined Pay" In RMA Application -->
	<test name="RMA_Reg_CombinedPayment_TC_04"> 
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_CombinedPayment_TC_04" />
		</classes>
	</test>
	
	<!--Carrier WC Claim Creation In RMA Application --> 
	<test name="RMA_Smoke_TC_075_CombinedPay">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_075" />
		</classes>
	</test>
	
	<!-- Reserve Creation On The Created Carrier Workers Compensation Claim -->
	<test name="RMA_Smoke_TC_053_CarrierWC_CombinedPay">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_053_CarrierWC" />
		</classes>
	</test>
	
	<!--Payment Creation For Stopped Combined Payment In RMA Application -->
	<test name="RMA_Reg_CombinedPaymentCarrierWC_TC_03_CombinedPay"> 
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_CombinedPaymentCarrierWC_TC_03" />
		</classes>
	</test>
	
	<!--Stop Combined Payment Functionality With Stop As Of  Date In RMA Application --> 
	<test name="RMA_Reg_CombinedPayment_TC_06"> 
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_CombinedPayment_TC_06" />
		</classes>
	</test>
	
	<!--Carrier WC Claim Creation In RMA Application --> 
	<test name="RMA_Smoke_TC_075_StopAsOf">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_075" />
		</classes>
	</test>
	
	<!-- Reserve Creation On The Created Carrier Workers Compensation Claim -->
	<test name="RMA_Smoke_TC_053_CarrierWC_StopAsOf">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_053_CarrierWC" />
		</classes>
	</test>
	
	<!--Payment Creation For Stopped Combined Payment In RMA Application -->
	<test name="RMA_Reg_CombinedPaymentCarrierWC_TC_03_StopAsOf"> 
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_CombinedPaymentCarrierWC_TC_03" />
		</classes>
	</test>
	
	<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>