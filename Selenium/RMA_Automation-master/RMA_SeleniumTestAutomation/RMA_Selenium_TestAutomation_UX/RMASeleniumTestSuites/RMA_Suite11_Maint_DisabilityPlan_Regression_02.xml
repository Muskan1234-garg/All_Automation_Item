<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Suite11_Maint_DisabilityPlan_Regression_02" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--Verify Maintenance Screen Navigation In RMA Application -->
	<test name="RMA_Smoke_TC_041_I">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_041_I" />
		</classes>
	</test>
	
	<!--Verify The Correct Validation When User Tries To Save A Record Without Entering Values In Mandatory Fields -->
	<test name="RMA_Reg_Maint_DisabilityPlan_TC_003">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_DisabilityPlan_TC_003" />
		</classes>
	</test>

	<!--Verify That All The Tool Bar Buttons Are Present And On Clicking The Tool bar Buttons For A New Plan A Validation Is Displayed That The User Is Working On New Record -->
	<test name="RMA_Reg_Maint_DisabilityPlan_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_DisabilityPlan_TC_001" />
		</classes>
	</test>

	<!--Verify That Correct Validation Message Is Displayed When User Enters Expiration Date Less Than Effective Date -->
	<test name="RMA_Reg_Maint_DisabilityPlan_TC_004">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_DisabilityPlan_TC_004" />
		</classes>
	</test>

	<!--Verify User Is Able To Save/Edit/Delete An Existing Plan -->
	<test name="RMA_Reg_Maint_DisabilityPlan_TC_006">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_DisabilityPlan_TC_006" />
		</classes>
	</test>
	
	<!--Verify Bank Account / Sub Bank Account List Is Displayed As Per The Setting Done In General System Parameter Under Funds -->
	<test name="RMA_Reg_Maint_DisabilityPlan_TC_007">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_DisabilityPlan_TC_007" />
		</classes>
	</test>	
	
<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>