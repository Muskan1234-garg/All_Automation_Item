<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName"
		value="RMA_Regression_EnhancedNotes_Claims_SMSScreens" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!-- Successful Corporate General Claim Creation -->
	<test name="RMA_Smoke_TC_002_GC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_GC" />
		</classes>
	</test>

	<!-- Enable/Disable Enhanced Note Setting For Claim and Claim's Child Screens 
		In SMS -->
	<test name="RMA_Reg_EnhancedNotes_TC_005_GC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_EnhancedNotes_TC_005" />
		</classes>
	</test>

	<!-- Successful Corporate Worker's Comp Claim Creation -->
	<test name="RMA_Smoke_TC_002_WC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_WC" />
		</classes>
	</test>

	<!-- Enable/Disable Enhanced Note Setting For Claim and Claim's Child Screens 
		In SMS -->
	<test name="RMA_Reg_EnhancedNotes_TC_005_WC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_EnhancedNotes_TC_005" />
		</classes>
	</test>

	<!-- Successful Corporate Vehicle Claim Creation -->
	<test name="RMA_Smoke_TC_002_VC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_VC" />
		</classes>
	</test>

	<!-- Enable/Disable Enhanced Note Setting For Claim and Claim's Child Screens 
		In SMS -->
	<test name="RMA_Reg_EnhancedNotes_TC_005_VC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_EnhancedNotes_TC_005" />
		</classes>
	</test>

	<!-- Successful Corporate Property Claim Creation -->
	<test name="RMA_Smoke_TC_002_PC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_PC" />
		</classes>
	</test>

	<!-- Enable/Disable Enhanced Note Setting For Claim and Claim's Child Screens 
		In SMS -->
	<test name="RMA_Reg_EnhancedNotes_TC_005_PC">
		-->
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_EnhancedNotes_TC_005" />
		</classes>
	</test>

	<!-- Verify the SMS setting for Enhanced Note For Event -->
	<test name="RMA_Reg_EnhancedNotes_TC_006">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_EnhancedNotes_TC_006" />
		</classes>
	</test>

	<!-- Verify Enhanced Notes Button Is Visible On The Loss Screen(Unit) Of 
		Vehicle Accident Claim And It Show The Enhanced Notes Of Parent Claim -->
	<test name="RMA_Reg_EnhancedNotes_TC_014">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_EnhancedNotes_TC_014" />
		</classes>
	</test>

	<!-- Verify Enhanced Notes Button Is Visible On The Loss Screen(Liability) 
		Of General Claim And It Show The Enhanced Notes Of Parent Claim -->
	<test name="RMA_Reg_EnhancedNotes_TC_015">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_EnhancedNotes_TC_015" />
		</classes>
	</test>

	<!-- Verify Enhanced Notes Button Is Visible On The Loss Screen(Property) 
		Of Carrier General Claim And It Show The Enhanced Notes Of Parent Claim -->
	<test name="RMA_Reg_EnhancedNotes_TC_016">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_EnhancedNotes_TC_016" />
		</classes>
	</test>


<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>