	<test name="RMA_Reg_Funds_ClearCheck_Carrier_TC_005">
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_ClearCheck_Carrier_TC_005" />
		</classes>
	</test>

	<!--Void Payments Are Not Visible On The Mark checks As Cleared Screen Is 
