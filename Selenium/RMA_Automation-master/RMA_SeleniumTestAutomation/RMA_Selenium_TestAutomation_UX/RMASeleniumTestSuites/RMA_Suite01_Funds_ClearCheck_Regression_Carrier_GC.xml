	<test name="RMA_Reg_Funds_ClearCheck_Carrier_TC_002">
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_ClearCheck_Carrier_TC_002" />
		</classes>
	</test>

	<!--Various Filters Like Bank account, Company, From Date, To Date and Include 
