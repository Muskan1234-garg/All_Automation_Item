		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_ClearCheck_Carrier_TC_004" />
		</classes>
	</test>

	<!--When Clear CheckBox Is Checked On Transaction, Then Check Is Not Visible 
		On Funds->Mark Check As Cleared Page, Cleared Date In Transaction History 
