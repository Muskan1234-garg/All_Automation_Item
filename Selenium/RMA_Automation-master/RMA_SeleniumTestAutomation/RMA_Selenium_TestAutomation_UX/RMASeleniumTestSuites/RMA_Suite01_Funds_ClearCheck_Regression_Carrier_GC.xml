<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName"
		value="RMA_Suite01_Funds_ClearCheck_Regression_Carrier_GC" />
<!-- Make sure that check void reason settings is off in Payment parameter setup -->
	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--General Claim Getting Created By Entering All Mandatory Fields Is Validated -->
	<test name="RMA_Smoke_TC_058">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_058" />
		</classes>
	</test>

	<!--Reserve Creation Is Validated -->
	<test name="RMA_Smoke_TC_053">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_053" />
		</classes>
	</test>

	<!--Creation Of Payments and Collection and Print Them By Various Methods 
		Is Validated -->
	<test name="RMA_Reg_Funds_ClearCheck_TC_Carrier_001">
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_ClearCheck_Carrier_TC_001" />
		</classes>
	</test>

	<!--Creation Of Payments and Collection and Print Them By Various Methods 
