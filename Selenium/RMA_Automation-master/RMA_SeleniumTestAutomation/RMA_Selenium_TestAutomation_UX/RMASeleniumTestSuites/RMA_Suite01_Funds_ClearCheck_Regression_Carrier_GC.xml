	<test name="RMA_Reg_Funds_ClearCheck_Carrier_TC_003">
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_ClearCheck_Carrier_TC_003" />
		</classes>
	</test>

	<!--Checks Not Attached to Claims CheckBox Function, Clear CheckBox On Transaction 
