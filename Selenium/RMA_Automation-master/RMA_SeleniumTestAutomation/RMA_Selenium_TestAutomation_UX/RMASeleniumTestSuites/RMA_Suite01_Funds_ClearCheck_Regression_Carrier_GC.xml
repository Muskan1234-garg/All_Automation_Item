	<test name="RMA_Reg_Funds_ClearCheck_Carrier_TC_006">
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_ClearCheck_Carrier_TC_006" />
		</classes>
	</test>

	<!--Visibility Of Printed AutoChecks On Clear Screen Is Validated -->
	<test name="RMA_Reg_Funds_ClearCheck_Carrier_TC_008">
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_ClearCheck_Carrier_TC_008" />
