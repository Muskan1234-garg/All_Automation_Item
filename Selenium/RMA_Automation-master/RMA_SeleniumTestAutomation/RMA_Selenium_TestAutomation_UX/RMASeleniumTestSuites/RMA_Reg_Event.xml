<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
<parameter name="DataSourceRow" value="2" />

<!--To Set The Executed SuiteName-->
<parameter name="ExecutedSuiteName" value="RMA_Reg_Event" />

<!--Verify That RMA Application Is Logged In-->
  <test name="RMA_TC_001">
    <classes>
      <class name="rmaseleniumtestscripts.RMA_TC_001"/>
    </classes>
  </test>
  
<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>
	
<!--: Successful Event Creation On Providing Values in Every Field of Every Accordion--> 
	<test name="RMA_Reg_Event_01">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Event_01" />
		</classes>
	</test>

<!-- Successful Event Creation of Event Category 'Fall'  -->
	<test name="RMA_Reg_Event_02">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Event_02" />
		</classes>
	</test>
	
<!-- Successful Event Creation of Event Category 'Medication'  -->
	<test name="RMA_Reg_Event_03">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Event_03" />
		</classes>
	</test>
	
	<!-- Successful Event Creation of Event Category 'Sentinel '  -->
	<test name="RMA_Reg_Event_04">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Event_04" />
		</classes>
	</test>

	<!-- Successful Event Creation of & verify 'Event On Premise' Checkbox functionality  -->
	<test name="RMA_Reg_Event_05">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Event_05" />
		</classes>
	</test>

	
<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>



	
</suite>