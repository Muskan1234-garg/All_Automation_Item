<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName"
		value="RMA_Regression_CombinedPayment_On_CarrierWC_01" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--Combined Payment Setup In RMA Application -->
 	<test name="RMA_QTPRegression_TC_052_I"> 
		<classes> 
		<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_052_I" /> 
	</classes> 
 	</test> 

	<!--Carrier Workers Compensation Claim Creation -->
	<test name="RMA_Smoke_TC_075">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_075" />
		</classes>
	</test>

	<!-- Reserve Creation On The Created Carrier Workers Compensation Claim -->
	<test name="RMA_Smoke_TC_053_CarrierWC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_053_CarrierWC" />
		</classes>
	</test>
	
 	<!--Combined Payment Creation On Created Carrier General Claim -->
	<test name="RMA_Reg_CombinedPaymentCarrierWC_TC_01">
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_CombinedPaymentCarrierWC_TC_01" />
		</classes>
	</test>

	<!-- Combined Collection Creation On Created Carrier General Claim -->
	<test name="RMA_Reg_CombinedPaymentCarrierWC_TC_02">
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_CombinedPaymentCarrierWC_TC_02" />
		</classes>
	</test>

	<!--Combined Auto Check Creation In RMA Application --> -->
	<test name="RMA_Reg_CombinedPaymentCarrierWC_TC_04"> 
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_CombinedPaymentCarrierWC_TC_04" />
		</classes>
	</test>
	
	<!--Verify That Multiple Combined Payments Will Be Rolled Up Even If Roll Up Checks To Same Payee Is Not Turned On-->
	<test name="RMA_Reg_CombinedPayment_TC_13_Carrier"> 
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_CombinedPayment_TC_13_Carrier" />
		</classes>
	</test>

<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>