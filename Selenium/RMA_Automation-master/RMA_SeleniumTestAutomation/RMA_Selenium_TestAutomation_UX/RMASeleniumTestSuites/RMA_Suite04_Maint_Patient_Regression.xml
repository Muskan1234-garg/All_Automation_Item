<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Suite04_Maint_Patient_Regression" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>
	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--Verify ToolBar Button On Patient Are Working Fine -->
	<test name="RMA_Reg_Maint_Patient_TC_006">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Patient_TC_006" />
		</classes>
	</test>
	<!-- Verify That User Is Able To Add New Supplemental Field On Patient Tracking 
		Screen -->
	<test name="RMA_Reg_Maint_Patient_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Patient_TC_001" />
		</classes>
	</test>
	<!--Verify That Phone Number Fields Are Present On Patient Tracking Screen -->
	<test name="RMA_Reg_Maint_Patient_TC_003">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Patient_TC_003" />
		</classes>
	</test>
	<!--Verify That User Is Able To Add Patient Created Through Maintenance 
		As PI On General Claim -->
	<test name="RMA_Reg_Maint_Patient_TC_004">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Patient_TC_004" />
		</classes>
	</test>
	<!--Verify That User Is Able To Add Procedure On Patient Along With All 
		the Complication Data And Other Info -->
	<test name="RMA_Reg_Maint_Patient_TC_005">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Patient_TC_005" />
		</classes>
	</test>
	<!--Verify That User Is Able To Multiple Addresses On Patient And Verify 
		Other Functionalities Of "Enables Multiple Addresses" -->
	<test name="RMA_Reg_Maint_Patient_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Patient_TC_002" />
		</classes>
	</test>
	<!--Verify That User Is Able To Multiple Addresses On Patient And Verify 
		Other Functionalities Of "Enables Multiple Addresses" -->
	<test name="RMA_Reg_Maint_Patient_TC_007">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Patient_TC_007" />
		</classes>
	</test>
<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>