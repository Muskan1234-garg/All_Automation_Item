	<!--Verify In General System Parameters When AutoNumber Property ID (PIN) Is Checked Then Property Id Is Automatically Generated On Property Creation Screen -->
	<test name="RMA_Reg_Maint_Property_TC_020">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Property_TC_020" />
		</classes>
	</test>
    
    <!-- Verify In General System Parameters When AutoNumber Property ID (PIN) Is Unchecked Then Property Id Is Given Manually On Property Creation Screen  -->
	<test name="RMA_Reg_Maint_Property_TC_021">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Property_TC_021" />
		</classes>
	</test>
	
    <!--Verify The Functionality Of View SMS Permission Setting For Property  -->
    <test name="RMA_Reg_Maint_Property_TC_017">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Property_TC_017" />
		</classes>
	</test>

    <!--Verify The Functionality Of Update SMS Permission Setting For Property-->
	<test name="RMA_Reg_Maint_Property_TC_019">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Property_TC_019" />
		</classes>
	</test>

<!--Verify The Functionality Of Tool Bar Buttons From From Maintenance Screen -->
	<test name="RMA_Reg_Maint_Property_TC_004">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Maint_Property_TC_004" />
		</classes>
	</test>

<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
