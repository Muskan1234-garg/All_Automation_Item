<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!-- Execute Policy System Setup Utility To Setup a Staging Policy System 
		And Financial Key Having Claimant,ReserveType,PolicyUnitCoverage,LossDisabilityType 
		As its Part -->

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Regression_RWS_FinancialKeys" />

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--Reserve WorkSheet Settings From Line Of Business Setup And Reserve WorkSheet 
		Configuration Setting From Utilities Is Validated -->
	<parameter name="LOB" value="GC - General Claims" />
	<parameter name="numofreserves" value="2" />
	<test name="RMA_Reg_Funds_ReserveWorkSheet_Settings">
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorkSheet_Settings" />
		</classes>
	</test>

	<!--Edit Of Established Financial Key From Utilities -->
	<test name="RMA_Reg_Funds_ReserveWorkSheet_EditFinancialKey">
		<parameter name="Claimant" value="check" />
		<parameter name="ReserveType" value="check" />
		<parameter name="ReserveSubType" value="uncheck" />
		<parameter name="PolicyUnitCoverage" value="check" />
		<parameter name="LossDisabilityType" value="check" />
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorkSheet_EditFinancialKey" />
		</classes>
	</test>

	<!--Successful Carrier General Claim Creation -->
	<test name="RMA_Reg_Funds_ReserveWorkSheet_TC_018">
		<parameter name="Type" value="Carrier" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorkSheet_TC_018" />
		</classes>
	</test>

	<!-- To Save a Reserve WorkSheet On A Claim Having Different Financial Keys -->
	<test name="RMA_Reg_Funds_ReserveWorkSheet_TC_009">
		<parameter name="LOB" value="GC" />
		<parameter name="PolicyType" value="External" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorkSheet_TC_009"></class>
		</classes>
	</test>

	<!-- Successful Claim Search And Then Delete Of The Claim Is Validated -->
	<test name="RMA_Smoke_TC_050">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_050" />
		</classes>
	</test>

	<!--Edit Of Established Financial Key From Utilities -->
	<test name="RMA_Reg_Funds_ReserveWorkSheet_EditFinancialKey_1">
		<parameter name="Claimant" value="check" />
		<parameter name="ReserveType" value="check" />
		<parameter name="ReserveSubType" value="check" />
		<parameter name="PolicyUnitCoverage" value="check" />
		<parameter name="LossDisabilityType" value="check" />
		<classes>
			<class
				name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorkSheet_EditFinancialKey" />
		</classes>
	</test>

	<!--Successful Carrier General Claim Creation -->
	<test name="RMA_Reg_Funds_ReserveWorkSheet_TC_018_1">
		<parameter name="Type" value="Carrier" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorkSheet_TC_018" />
		</classes>
	</test>

