	<!--Successful Corporate General Claim Creation -->
	<test name="RMA_Reg_Funds_ReserveWorkSheet_TC_018_3">
		<parameter name="Type" value="Corporate" />
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ReserveWorkSheet_TC_018" />
		</classes>
