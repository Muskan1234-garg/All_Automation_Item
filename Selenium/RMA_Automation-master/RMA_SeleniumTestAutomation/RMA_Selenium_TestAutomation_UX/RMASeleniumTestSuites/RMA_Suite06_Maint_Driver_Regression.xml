<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!-- To Execute this suite turn on Paging from Grid Configuration,enable 
		PI as PayeeType Setting and make sure test01 user is present -->

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Suite06_Maint_Driver_Regression" />
	<!-- This Suite Can Be Run On 132 Server. All Uncommented Test Cases Are 
		Tested on It. -->
	<!-- To make this suite run on 132 server, please change DataSheet for test 
		cases RMA_Reg_Maint_Driver_TC_007,008,010 -->
	<!--Verify That RMA Application Is Logged In -->

	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>
	<!--Verify That Required DSN Is Selected In RMA Application -->
