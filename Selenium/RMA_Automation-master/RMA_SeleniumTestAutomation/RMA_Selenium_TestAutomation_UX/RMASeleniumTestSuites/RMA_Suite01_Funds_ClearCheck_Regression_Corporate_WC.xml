<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="1" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName"
		value="RMA_Suite01_Funds_ClearCheck_Regression_Corporate_WC" />
<!-- Make sure that check void reason settings is off in Payment parameter setup -->
	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>

	<!--Worker's Compensation Claim Getting Created By Entering All Mandatory 
		Fields Is Validated -->
	<test name="RMA_Smoke_TC_002_WC">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Smoke_TC_002_WC" />
		</classes>
	</test>

	<!--Creation Of Payments and Collection and Print Them By Various Methods 
		Is Validated -->
	<test name="RMA_Reg_Funds_ClearCheck_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ClearCheck_TC_001" />
		</classes>
	</test>

	<!--Creation Of Payments and Collection and Print Them By Various Methods 
		Is Validated -->
	<test name="RMA_Reg_Funds_ClearCheck_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ClearCheck_TC_002" />
		</classes>
	</test>

	<!--Various Filters Like Bank account, Company, From Date, To Date and Include 
		Collections On Mark Page As Cleared Are Validated -->
	<test name="RMA_Reg_Funds_ClearCheck_TC_003">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ClearCheck_TC_003" />
		</classes>
	</test>

	<!--Checks Not Attached to Claims CheckBox Function, Clear CheckBox On Transaction 
		Is Validated -->
	<test name="RMA_Reg_Funds_ClearCheck_TC_004">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ClearCheck_TC_004" />
		</classes>
	</test>

	<!--When Clear CheckBox Is Checked On Transaction, Then Check Is Not Visible 
		On Funds->Mark Check As Cleared Page, Cleared Date In Transaction History 
		and Funds History Is Validated -->
	<test name="RMA_Reg_Funds_ClearCheck_TC_005">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ClearCheck_TC_005" />
		</classes>
	</test>

	<!--Void Payments Are Not Visible On The Mark checks As Cleared Screen Is 
		Validated -->
	<test name="RMA_Reg_Funds_ClearCheck_TC_006">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ClearCheck_TC_006" />
		</classes>
	</test>

	<!--Visibility Of Printed AutoChecks On Clear Screen Is Validated -->
	<test name="RMA_Reg_Funds_ClearCheck_TC_008">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ClearCheck_TC_008" />
		</classes>
	</test>

	<!--Visibility Of Printed Final/Supplemental Payment On Clear Screen Is 
		Validated -->
	<test name="RMA_Reg_Funds_ClearCheck_TC_009">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ClearCheck_TC_009" />
		</classes>
	</test>

	<!--Combined Payment Setup -->
	<test name="RMA_QTPRegression_TC_052_I">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_052_I" />
		</classes>
	</test>

	<!--Visibility Of Printed Combined Payment On Clear Screen Is Validated -->
	<test name="RMA_Reg_Funds_ClearCheck_TC_010">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Funds_ClearCheck_TC_010" />
		</classes>
	</test>
<!-- Logout From Application At end -->
	<test name="RMA_Reg_Logout">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_Reg_Logout" />
		</classes>
	</test>
	</suite>