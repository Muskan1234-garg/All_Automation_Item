<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
	<parameter name="DataSourceRow" value="3" />

	<!--To Set The Executed SuiteName -->
	<parameter name="ExecutedSuiteName" value="RMA_Suite_20_QTPReg_09_ResCal"/>

	<!--Verify That RMA Application Is Logged In -->
	<test name="RMA_TC_001">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_001" />
		</classes>
	</test>

	<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>
	
<!-- Verify new tab 'Reserve/Incurred Balance Options' is created in LOB Setup --> 	
	<test name="RMA_QTPRegression_TC_035">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_035" />
		</classes>
	</test>
	
	<!--Verify validation message on selecting same reserve type for 'Calculate Collections in Reserve Balance Per Reserve' and 'Calculate Collections in Incurred Balance Per Reserve'-->
	<test name="RMA_QTPRegression_TC_043">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_043" />
		</classes> 
	</test>
	
	<!--Verify Reserve and Incurred Balance calculations with selection of checkbox 'Calculate Collections in Reserve Balance-->
	<test name="RMA_QTPRegression_TC_038">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_038" />
		</classes>
	</test>
	
	<!--Verify Reserve and Incurred Balance calculations with selection of checkbox 'Calculate Collections in Incurred Balance'-->
	<test name="RMA_QTPRegression_TC_039">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_039" />
		</classes>
	</test> 
	
	<!--Verify Reserve and Incurred Balance calculations with selection of checkbox 'Calculate Collections in Reserve Balance Per Reserve'-->
	<test name="RMA_QTPRegression_TC_040">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_040" />
		</classes>
	</test>
	
	<!--Verify Reserve and Incurred Balance calculations with selection of checkbox 'Calculate Collections in Incurred Balance Per Reserve'-->
	<test name="RMA_QTPRegression_TC_041">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_041" />
		</classes> 
	</test>
	
		<!--Reserve And Incurred Balance Calculations With Selection Of  'Calculate Collections In Reserve Balance Per Reserve' And 'Calculate Collections In Incurred Balance Per Reserve' Checkbox Is Validated-->
	<test name="RMA_QTPRegression_TC_042">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_042" />
		</classes> 
	</test>		
	
	<!--Verify Incurred Handling Options : Add settings for Recovery Reserves to add their Recovery Incurred Amount to the Total Claim Incurred Amount or subtract their Recovery Incurred Amount from the Total Claim Incurred Amount-->
	<test name="RMA_QTPRegression_TC_036">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_036" />
		</classes>
	</test>
	
		<!--Verify Incurred Handling Options : Add settings for Recovery Reserves to add their Recovery Reserve Balances to the Total Claim Reserve Balance or subtract their Recovery Reserve Balances from the Total Claim Reserve Balance-->
	<test name="RMA_QTPRegression_TC_037">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_QTPRegression_TC_037" />
		</classes>
	</test>		
</suite>
