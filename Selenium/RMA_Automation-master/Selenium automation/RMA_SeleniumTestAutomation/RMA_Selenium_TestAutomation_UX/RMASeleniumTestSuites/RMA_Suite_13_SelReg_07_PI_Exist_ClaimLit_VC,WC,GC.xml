<suite name="RMA_SampleSuite" parallel="none" allow-return-values="true">
<parameter name="DataSourceRow" value="1" />

<!--To Set The Executed SuiteName-->
<parameter name="ExecutedSuiteName" value="RMA_Suite_13_SelReg_07_PI_Exist_ClaimLit_VC,WC,GC" />
<!-- NOTE: Other People Add New Person Involved Script (RMA_SeleniumRegression_TC_040) Should Be Run At The End Of Each Suite Execution As It Contains The Code To Close The Claim Screen -->


<!--Verify That RMA Application Is Logged In-->
  <test name="RMA_TC_001">
    <classes>
      <class name="rmaseleniumtestscripts.RMA_TC_001"/>
    </classes>
  </test>
  
<!--Verify That Required DSN Is Selected In RMA Application -->
	<test name="RMA_TC_002">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_TC_002" />
		</classes>
	</test>
	

	
<!--Existence Of Person Involved Entity Option For Saved Vehicle Accident Claim Is Validated -->
	 <test name="RMA_SeleniumRegression_TC_022_General Claim">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_SeleniumRegression_TC_022" />
		</classes>
	</test>
	
<!--  New Person Involved For Created Claim/Event/Litigation Is Added To Be Used As Add Existing Person Involved Data For Other Entity Level Such As (For Instance: Employee Created As Person Involved For Event Will Be Used As Data For Add Existing Person Involved For General Claim) -->
	 <test name="RMA_SeleniumRegression_TC_034_PI_DataCreation_General Claim">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_SeleniumRegression_TC_034_PI_DataCreation" />
		</classes>
	</test>
	
<!--Existence Of Person Involved Entity Option For Saved General Claim Is Validated -->
	 <test name="RMA_SeleniumRegression_TC_021_General Claim">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_SeleniumRegression_TC_021" />
		</classes>
	</test>
	
<!--Existence Of Person Involved Entity Option For Saved General Claim Litigation Is Validated  -->
	 <test name="RMA_SeleniumRegression_TC_024_General Claim_Litigation">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_SeleniumRegression_TC_024" />
		</classes>
	</test>
	
<!---Create, Edit And Delete Of Medical Staff As Person Involved For Created Claim/Event/Litigation Is Validated  -->
	 <test name="RMA_SeleniumRegression_TC_034_General Claim">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_SeleniumRegression_TC_034" />
		</classes>
	</test>
	
<!---Create, Edit And Delete Of Patient As Person Involved For Created Claim/Event/Litigation Is Validated  -->
	 <test name="RMA_SeleniumRegression_TC_035_General Claim">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_SeleniumRegression_TC_035" />
		</classes>
	</test>
	
<!---Create, Edit And Delete Of Physician As Person Involved For Created Claim/Event/Litigation Is Validated  -->
	 <test name="RMA_SeleniumRegression_TC_036_General Claim">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_SeleniumRegression_TC_036" />
		</classes>
	</test>
	
<!---Create, Edit And Delete Of Driver As Person Involved For Created Claim/Event/Litigation Is Validated  -->
	 <test name="RMA_SeleniumRegression_TC_037_General Claim">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_SeleniumRegression_TC_037" />
		</classes>
	</test>
	
<!---Create, Edit And Delete Of Witness As Person Involved For Created Claim/Event/Litigation Is Validated  -->
	 <test name="RMA_SeleniumRegression_TC_038_General Claim">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_SeleniumRegression_TC_038" />
		</classes>
	</test>
	
<!---Create, Edit And Delete Of Employee As Person Involved For Created Claim/Event/Litigation Is Validated  -->
	 <test name="RMA_SeleniumRegression_TC_039_General Claim">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_SeleniumRegression_TC_039" />
		</classes>
	</test>
	
<!---Create, Edit And Delete Of Other Person As Person Involved For Created Claim/Event/Litigation Is Validated  -->
	 <test name="RMA_SeleniumRegression_TC_040_General Claim">
		<classes>
			<class name="rmaseleniumtestscripts.RMA_SeleniumRegression_TC_040" />
		</classes>
	</test>
</suite>